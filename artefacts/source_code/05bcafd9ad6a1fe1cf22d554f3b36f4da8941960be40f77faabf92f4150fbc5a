public void stop(){
    super.stop();
    if (DEBUG) {
        log("stop");
    }
    if (!isInitialized()) {
        return;
    }
    if (mTaskToken == null) {
        return;
    }
    WindowContainerTransaction wct = new WindowContainerTransaction();
    wct.setHidden(mTaskToken, true);
    WindowOrganizer.applyTransaction(wct);
    if (mListener != null) {
        mListener.onTaskVisibilityChanged(getTaskId(), false);
    }
}