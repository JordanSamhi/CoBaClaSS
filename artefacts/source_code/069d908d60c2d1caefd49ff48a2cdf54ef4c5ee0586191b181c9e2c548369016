public boolean equals(Object that){
    if (this == that)
        return true;
    if (!(that instanceof Matrix))
        return false;
    Matrix other = (Matrix) that;
    if (n != other.n)
        return false;
    if (m != other.m)
        return false;
    for (int i = 0; i < mem.length; i++) {
        if (mem[i] != other.mem[i])
            return false;
    }
    return true;
}