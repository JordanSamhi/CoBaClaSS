public String getSSID(){
    if (mWifiSsid != null) {
        String unicode = mWifiSsid.toString();
        if (!TextUtils.isEmpty(unicode)) {
            return "\"" + unicode + "\"";
        } else {
            String hex = mWifiSsid.getHexString();
            return (hex != null) ? hex : WifiManager.UNKNOWN_SSID;
        }
    }
    return WifiManager.UNKNOWN_SSID;
}