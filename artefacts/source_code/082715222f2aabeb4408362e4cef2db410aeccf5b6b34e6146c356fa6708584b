public SignalStrength getSignalStrength(){
    try {
        ITelephony service = getITelephony();
        if (service != null) {
            return service.getSignalStrength(getSubId());
        }
    } catch (RemoteException e) {
        Log.e(TAG, "Error calling ITelephony#getSignalStrength", e);
    }
    return null;
}