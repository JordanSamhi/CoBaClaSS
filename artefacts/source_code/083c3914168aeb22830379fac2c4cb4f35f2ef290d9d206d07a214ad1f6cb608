public static String getReasonCodeDescription(int reasonCode){
    switch(reasonCode) {
        case REASON_CANCELED:
            return "canceled";
        case REASON_CONSTRAINTS_NOT_SATISFIED:
            return "constraints";
        case REASON_PREEMPT:
            return "preempt";
        case REASON_TIMEOUT:
            return "timeout";
        case REASON_DEVICE_IDLE:
            return "device_idle";
        case REASON_DEVICE_THERMAL:
            return "thermal";
        case REASON_RESTRICTED_BUCKET:
            return "restricted_bucket";
        default:
            return "unknown:" + reasonCode;
    }
}