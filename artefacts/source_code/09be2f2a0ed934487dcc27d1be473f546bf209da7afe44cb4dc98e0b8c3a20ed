public WindowInsets dispatchWindowInsetsAnimationProgress(@NonNull WindowInsets insets, @NonNull List<WindowInsetsAnimation> runningAnimations){
    if (mListenerInfo != null && mListenerInfo.mWindowInsetsAnimationCallback != null) {
        return mListenerInfo.mWindowInsetsAnimationCallback.onProgress(insets, runningAnimations);
    } else {
        return insets;
    }
}