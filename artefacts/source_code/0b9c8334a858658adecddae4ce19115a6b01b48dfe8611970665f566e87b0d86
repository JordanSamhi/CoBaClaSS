public void agentConnected(String packageName, IBinder agentBinder){
    synchronized (mAgentConnectLock) {
        if (Binder.getCallingUid() == Process.SYSTEM_UID) {
            Slog.d(TAG, addUserIdToLogMessage(mUserId, "agentConnected pkg=" + packageName + " agent=" + agentBinder));
            mConnectedAgent = IBackupAgent.Stub.asInterface(agentBinder);
            mConnecting = false;
        } else {
            Slog.w(TAG, addUserIdToLogMessage(mUserId, "Non-system process uid=" + Binder.getCallingUid() + " claiming agent connected"));
        }
        mAgentConnectLock.notifyAll();
    }
}