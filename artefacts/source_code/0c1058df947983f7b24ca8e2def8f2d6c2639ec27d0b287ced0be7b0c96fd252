public static VibrationEffect get(Uri uri, Context context){
    String[] uris = context.getResources().getStringArray(com.android.internal.R.array.config_ringtoneEffectUris);
    if (uris.length == 0) {
        return null;
    }
    final ContentResolver cr = context.getContentResolver();
    Uri uncanonicalUri = cr.uncanonicalize(uri);
    if (uncanonicalUri == null) {
        uncanonicalUri = uri;
    }
    for (int i = 0; i < uris.length && i < RINGTONES.length; i++) {
        if (uris[i] == null) {
            continue;
        }
        Uri mappedUri = cr.uncanonicalize(Uri.parse(uris[i]));
        if (mappedUri == null) {
            continue;
        }
        if (mappedUri.equals(uncanonicalUri)) {
            return get(RINGTONES[i]);
        }
    }
    return null;
}