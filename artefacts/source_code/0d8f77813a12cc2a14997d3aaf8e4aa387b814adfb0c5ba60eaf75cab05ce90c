public boolean addGnssAntennaInfoListener(IGnssAntennaInfoListener listener, String packageName, @Nullable String featureId){
    synchronized (mGnssAntennaInfoListeners) {
        return addGnssDataListenerLocked(null, listener, packageName, featureId, mGnssAntennaInfoProvider, mGnssAntennaInfoListeners, this::removeGnssAntennaInfoListener);
    }
}