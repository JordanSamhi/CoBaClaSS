public static boolean startViewCalendarEventInManagedProfile(@NonNull Context context, long eventId, long startMs, long endMs, boolean allDay, int flags){
    Preconditions.checkNotNull(context, "Context is null");
    final DevicePolicyManager dpm = (DevicePolicyManager) context.getSystemService(Context.DEVICE_POLICY_SERVICE);
    return dpm.startViewCalendarEventInManagedProfile(eventId, startMs, endMs, allDay, flags);
}