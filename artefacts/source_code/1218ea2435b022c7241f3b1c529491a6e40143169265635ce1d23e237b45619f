public Builder setInterpolatorType(@InterpolatorType int interpolatorType){
    switch(interpolatorType) {
        case INTERPOLATOR_TYPE_STEP:
        case INTERPOLATOR_TYPE_LINEAR:
        case INTERPOLATOR_TYPE_CUBIC:
        case INTERPOLATOR_TYPE_CUBIC_MONOTONIC:
            mInterpolatorType = interpolatorType;
            break;
        default:
            throw new IllegalArgumentException("invalid interpolatorType: " + interpolatorType);
    }
    return this;
}