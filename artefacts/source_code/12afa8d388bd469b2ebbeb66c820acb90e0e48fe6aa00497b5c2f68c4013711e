public Float getDefaultPeakRefreshRate(){
    float defaultPeakRefreshRate = DeviceConfig.getFloat(DeviceConfig.NAMESPACE_DISPLAY_MANAGER, DisplayManager.DeviceConfig.KEY_PEAK_REFRESH_RATE_DEFAULT, -1);
    if (defaultPeakRefreshRate == -1) {
        return null;
    }
    return defaultPeakRefreshRate;
}