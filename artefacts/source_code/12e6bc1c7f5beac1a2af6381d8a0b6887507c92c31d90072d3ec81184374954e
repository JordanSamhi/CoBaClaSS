public boolean matchesMccMnc(String mccMnc){
    if (mccMnc == null) {
        return false;
    }
    if (mccMnc.length() != MCC_MNC_LENGTH_5 && mccMnc.length() != MCC_MNC_LENGTH_6) {
        return false;
    }
    if (mPrefix && mccMnc.length() != mImsi.length()) {
        return false;
    }
    return mImsi.startsWith(mccMnc);
}