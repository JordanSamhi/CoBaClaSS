public boolean matchesCurrentSimOperator(@NonNull String mccmnc, @MvnoType int mvnoType, @Nullable String mvnoMatchData){
    try {
        if (!mccmnc.equals(getSimOperator())) {
            return false;
        }
        ITelephony service = getITelephony();
        if (service != null) {
            return service.isMvnoMatched(getSubId(), mvnoType, mvnoMatchData);
        }
    } catch (RemoteException ex) {
        if (!isSystemProcess()) {
            ex.rethrowAsRuntimeException();
        }
    }
    return false;
}