public boolean registerOnDestroyedCallback(@NonNull @CallbackExecutor Executor executor, @NonNull Runnable callback){
    Objects.requireNonNull(executor);
    Objects.requireNonNull(callback);
    if (isDestroyed()) {
        Log.w(TAG, "Cannot interact with a destroyed voice interactor");
        return false;
    }
    mOnDestroyCallbacks.put(callback, executor);
    return true;
}