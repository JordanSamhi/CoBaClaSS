public boolean addGnssNavigationMessageListener(IGnssNavigationMessageListener listener, String packageName, @Nullable String featureId){
    synchronized (mGnssNavigationMessageListeners) {
        return addGnssDataListenerLocked(null, listener, packageName, featureId, mGnssNavigationMessageProvider, mGnssNavigationMessageListeners, this::removeGnssNavigationMessageListener);
    }
}