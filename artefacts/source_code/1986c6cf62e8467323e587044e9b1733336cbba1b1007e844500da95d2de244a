public SoftApManager makeSoftApManager(@NonNull ActiveModeManager.Listener listener, @NonNull WifiManager.SoftApCallback callback, @NonNull SoftApModeConfiguration config){
    return new SoftApManager(mContext, mWifiHandlerThread.getLooper(), mFrameworkFacade, mWifiNative, mCountryCode.getCountryCode(), listener, callback, mWifiApConfigStore, config, mWifiMetrics, mSarManager, mWifiDiagnostics);
}