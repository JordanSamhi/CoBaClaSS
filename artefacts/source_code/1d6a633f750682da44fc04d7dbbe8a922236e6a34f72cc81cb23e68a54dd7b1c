public boolean isSetMacAddressSupported(@NonNull String ifaceName){
    synchronized (sLock) {
        android.hardware.wifi.V1_2.IWifiStaIface sta12 = getWifiStaIfaceForV1_2Mockable(ifaceName);
        if (sta12 != null) {
            return true;
        }
        android.hardware.wifi.V1_4.IWifiApIface ap14 = getWifiApIfaceForV1_4Mockable(ifaceName);
        if (ap14 != null) {
            return true;
        }
    }
    return false;
}