public int[] getDisableFlags(IBinder token, int userId){
    enforceStatusBar();
    int disable1 = 0;
    int disable2 = 0;
    synchronized (mLock) {
        DisableRecord record = findMatchingRecordLocked(token, userId).second;
        if (record != null) {
            disable1 = record.what1;
            disable2 = record.what2;
        }
    }
    return new int[] { disable1, disable2 };
}