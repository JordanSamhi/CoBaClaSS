public void answer(int videoState){
    Log.startSession("RC.an2", getActiveOwnerInfo());
    try {
        if (mConnected) {
            mConnectionService.answerVideo(mConnectionId, videoState, Log.getExternalSession(mCallingPackageAbbreviation));
        }
    } catch (RemoteException ignored) {
    } finally {
        Log.endSession();
    }
}