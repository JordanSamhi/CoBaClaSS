public String getTypeAllocationCode(int slotIndex){
    ITelephony telephony = getITelephony();
    if (telephony == null)
        return null;
    try {
        return telephony.getTypeAllocationCodeForSlot(slotIndex);
    } catch (RemoteException ex) {
        return null;
    } catch (NullPointerException ex) {
        return null;
    }
}