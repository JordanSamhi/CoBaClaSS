public void setForwardedInsets(Insets insets){
    if (insets == null) {
        insets = Insets.NONE;
    }
    if (mDisplayPolicy.getForwardedInsets().equals(insets)) {
        return;
    }
    mDisplayPolicy.setForwardedInsets(insets);
    setLayoutNeeded();
    mWmService.mWindowPlacerLocked.requestTraversal();
}