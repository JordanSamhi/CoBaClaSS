public boolean equals(Object o){
    if (this == o)
        return true;
    if (!(o instanceof CapabilityPair))
        return false;
    CapabilityPair that = (CapabilityPair) o;
    if (getCapability() != that.getCapability())
        return false;
    return getRadioTech() == that.getRadioTech();
}