public boolean getSegment(float startD, float stopD, Path dst, boolean startWithMoveTo){
    float length = getLength();
    if (startD < 0) {
        startD = 0;
    }
    if (stopD > length) {
        stopD = length;
    }
    if (startD >= stopD) {
        return false;
    }
    return native_getSegment(native_instance, startD, stopD, dst.mutateNI(), startWithMoveTo);
}