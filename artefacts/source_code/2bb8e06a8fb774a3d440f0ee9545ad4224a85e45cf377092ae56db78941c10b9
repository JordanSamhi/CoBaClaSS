public int[] encapsulationMetadataTypes(){
    if (mEncapsulationMetadataTypes == null) {
        return new int[0];
    }
    int[] encapsulationMetadataTypes = new int[mEncapsulationMetadataTypes.length];
    System.arraycopy(mEncapsulationMetadataTypes, 0, encapsulationMetadataTypes, 0, mEncapsulationMetadataTypes.length);
    return encapsulationMetadataTypes;
}