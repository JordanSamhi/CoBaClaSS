public List<Key<?>> getKeysNeedingPermission(){
    if (mKeysNeedingPermission == null) {
        Object crKey = CameraCharacteristics.Key.class;
        Class<CameraCharacteristics.Key<?>> crKeyTyped = (Class<CameraCharacteristics.Key<?>>) crKey;
        int[] filterTags = get(REQUEST_CHARACTERISTIC_KEYS_NEEDING_PERMISSION);
        if (filterTags == null) {
            mKeysNeedingPermission = Collections.unmodifiableList(new ArrayList<CameraCharacteristics.Key<?>>());
            return mKeysNeedingPermission;
        }
        mKeysNeedingPermission = getAvailableKeyList(CameraCharacteristics.class, crKeyTyped, filterTags, false);
    }
    return mKeysNeedingPermission;
}