public boolean renderSuggestion(int width, int height, @NonNull IInlineSuggestionUiCallback callback){
    if (mRemoteRenderService != null) {
        if (sDebug)
            Slog.d(TAG, "Request to recreate the UI");
        mRemoteRenderService.renderSuggestion(callback, mInlinePresentation, width, height, mHostInputToken, mDisplayId, mUserId, mSessionId);
        return true;
    }
    return false;
}