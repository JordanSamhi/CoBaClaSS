public Intent getDataManagementIntent(String transportName){
    mContext.enforceCallingOrSelfPermission(android.Manifest.permission.BACKUP, "getDataManagementIntent");
    try {
        Intent intent = mTransportManager.getTransportDataManagementIntent(transportName);
        if (MORE_DEBUG) {
            Slog.d(TAG, addUserIdToLogMessage(mUserId, "getDataManagementIntent() returning intent " + intent));
        }
        return intent;
    } catch (TransportNotRegisteredException e) {
        Slog.e(TAG, addUserIdToLogMessage(mUserId, "Unable to get management intent from transport: " + e.getMessage()));
        return null;
    }
}