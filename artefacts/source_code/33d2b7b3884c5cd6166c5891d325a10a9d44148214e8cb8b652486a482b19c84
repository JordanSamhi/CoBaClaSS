public CellLocation getCellLocation(){
    try {
        ITelephony telephony = getITelephony();
        if (telephony == null) {
            Rlog.d(TAG, "getCellLocation returning null because telephony is null");
            return null;
        }
        CellIdentity cellIdentity = telephony.getCellLocation(mContext.getOpPackageName(), mContext.getAttributionTag());
        CellLocation cl = cellIdentity.asCellLocation();
        if (cl == null || cl.isEmpty()) {
            Rlog.d(TAG, "getCellLocation returning null because CellLocation is empty or" + " phone type doesn't match CellLocation type");
            return null;
        }
        return cl;
    } catch (RemoteException ex) {
        Rlog.d(TAG, "getCellLocation returning null due to RemoteException " + ex);
        return null;
    }
}