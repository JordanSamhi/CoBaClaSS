public void getSmdsAddress(AsyncResultCallback<String> callback, Handler handler){
    sendApdu(newRequestProvider((RequestBuilder requestBuilder) -> requestBuilder.addStoreData(Asn1Node.newBuilder(Tags.TAG_GET_CONFIGURED_ADDRESSES).build().toHex())), (byte[] response) -> parseResponse(response).getChild(Tags.TAG_CTX_1).asString(), callback, handler);
}