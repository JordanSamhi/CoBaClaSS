public void stop(){
    if (DEBUG) {
        Log.d(mTag, "stop");
    }
    synchronized (mCmdQueue) {
        if (mState != STOP) {
            Command cmd = new Command();
            cmd.requestTime = SystemClock.uptimeMillis();
            cmd.code = STOP;
            enqueueLocked(cmd);
            mState = STOP;
        }
    }
}