public void clear(){
    bucketStart = EmptyArray.LONG;
    if (activeTime != null)
        activeTime = EmptyArray.LONG;
    if (rxBytes != null)
        rxBytes = EmptyArray.LONG;
    if (rxPackets != null)
        rxPackets = EmptyArray.LONG;
    if (txBytes != null)
        txBytes = EmptyArray.LONG;
    if (txPackets != null)
        txPackets = EmptyArray.LONG;
    if (operations != null)
        operations = EmptyArray.LONG;
    bucketCount = 0;
    totalBytes = 0;
}