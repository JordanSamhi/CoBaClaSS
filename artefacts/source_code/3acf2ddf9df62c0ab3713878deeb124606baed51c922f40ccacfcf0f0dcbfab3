public void acceptHandover(Uri srcAddr, @VideoProfile.VideoState int videoState, PhoneAccountHandle destAcct){
    try {
        if (isServiceConnected()) {
            getTelecomService().acceptHandover(srcAddr, videoState, destAcct);
        }
    } catch (RemoteException e) {
        Log.e(TAG, "RemoteException acceptHandover: " + e);
    }
}