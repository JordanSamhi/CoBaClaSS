public void injectEvents(List<GestureStep> gestureSteps, IAccessibilityServiceClient serviceInterface, int sequence, int displayId){
    SomeArgs args = SomeArgs.obtain();
    args.arg1 = gestureSteps;
    args.arg2 = serviceInterface;
    args.argi1 = sequence;
    args.argi2 = displayId;
    mHandler.sendMessage(mHandler.obtainMessage(MESSAGE_INJECT_EVENTS, args));
}