public OffsetResult lookupByOffsetWithBias(long whenMillis, @Nullable TimeZone bias, int totalOffsetMillis, boolean isDst){
    libcore.timezone.CountryTimeZones.OffsetResult delegateOffsetResult = mDelegate.lookupByOffsetWithBias(whenMillis, bias, totalOffsetMillis, isDst);
    return delegateOffsetResult == null ? null : new OffsetResult(delegateOffsetResult.getTimeZone(), delegateOffsetResult.isOnlyMatch());
}