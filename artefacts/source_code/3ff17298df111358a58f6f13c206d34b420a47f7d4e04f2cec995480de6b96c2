public boolean startDppConfiguratorInitiator(@NonNull String ifaceName, int peerBootstrapId, int ownBootstrapId, @NonNull String ssid, String password, String psk, int netRole, int securityAkm){
    final String methodStr = "startDppConfiguratorInitiator";
    if (!isV1_2()) {
        Log.e(TAG, "Method " + methodStr + " is not supported in existing HAL");
        return false;
    }
    ISupplicantStaIface iface = checkSupplicantStaIfaceAndLogFailure(ifaceName, methodStr);
    if (iface == null) {
        return false;
    }
    android.hardware.wifi.supplicant.V1_2.ISupplicantStaIface staIfaceV12 = getStaIfaceMockableV1_2(iface);
    if (staIfaceV12 == null) {
        Log.e(TAG, methodStr + ": ISupplicantStaIface is null");
        return false;
    }
    try {
        SupplicantStatus status = staIfaceV12.startDppConfiguratorInitiator(peerBootstrapId, ownBootstrapId, ssid, password != null ? password : "", psk != null ? psk : "", netRole, securityAkm);
        return checkStatusAndLogFailure(status, methodStr);
    } catch (RemoteException e) {
        handleRemoteException(e, methodStr);
    }
    return false;
}