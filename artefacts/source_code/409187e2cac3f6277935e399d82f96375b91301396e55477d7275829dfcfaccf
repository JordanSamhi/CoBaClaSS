public static String getShortcutTargetServiceComponentNameString(Context context, int userId){
    final String currentShortcutServiceId = Settings.Secure.getStringForUser(context.getContentResolver(), Settings.Secure.ACCESSIBILITY_SHORTCUT_TARGET_SERVICE, userId);
    if (currentShortcutServiceId != null) {
        return currentShortcutServiceId;
    }
    return context.getString(R.string.config_defaultAccessibilityService);
}