public boolean equals(final Object obj){
    if (obj == null) {
        return false;
    }
    if (this == obj) {
        return true;
    }
    if (obj instanceof RecommendedStreamConfiguration) {
        final RecommendedStreamConfiguration other = (RecommendedStreamConfiguration) obj;
        return mFormat == other.mFormat && mWidth == other.mWidth && mHeight == other.mHeight && mUsecaseBitmap == other.mUsecaseBitmap && mInput == other.mInput;
    }
    return false;
}