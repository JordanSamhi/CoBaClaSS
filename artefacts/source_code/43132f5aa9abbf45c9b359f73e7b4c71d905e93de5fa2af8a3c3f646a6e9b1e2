public NetworkCapabilities getNetworkCapabilities(String apnType){
    if (mTransportManager != null) {
        int transportType = mTransportManager.getCurrentTransport(ApnSetting.getApnTypesBitmaskFromString(apnType));
        if (getDcTracker(transportType) != null) {
            return getDcTracker(transportType).getNetworkCapabilities(apnType);
        }
    }
    return null;
}