public static String scoAudioModeToString(int scoAudioMode){
    switch(scoAudioMode) {
        case SCO_MODE_UNDEFINED:
            return "SCO_MODE_UNDEFINED";
        case SCO_MODE_VIRTUAL_CALL:
            return "SCO_MODE_VIRTUAL_CALL";
        case SCO_MODE_RAW:
            return "SCO_MODE_RAW";
        case SCO_MODE_VR:
            return "SCO_MODE_VR";
        default:
            return "SCO_MODE_(" + scoAudioMode + ")";
    }
}