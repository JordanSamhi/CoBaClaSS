public boolean sameCodecSpecificParameters(BluetoothCodecConfig other){
    if (other == null && mCodecType != other.mCodecType) {
        return false;
    }
    switch(mCodecType) {
        case SOURCE_CODEC_TYPE_LDAC:
            if (mCodecSpecific1 != other.mCodecSpecific1) {
                return false;
            }
        default:
            return true;
    }
}