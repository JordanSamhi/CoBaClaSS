public ChunksMetadataProto.ChunkListing getNewChunkListing(@Nullable byte[] fingerprintMixerSalt){
    mNewChunkListing.fingerprintMixerSalt = fingerprintMixerSalt != null ? Arrays.copyOf(fingerprintMixerSalt, fingerprintMixerSalt.length) : new byte[0];
    mNewChunkListing.chunks = mKnownChunks.toArray(new ChunksMetadataProto.Chunk[0]);
    return mNewChunkListing;
}