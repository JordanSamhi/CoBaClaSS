public boolean isValid(){
    if (mUniqueId == null) {
        return false;
    }
    final int count = mRoutes.size();
    for (int i = 0; i < count; i++) {
        MediaRoute2Info route = mRoutes.valueAt(i);
        if (route == null || !route.isValid()) {
            return false;
        }
    }
    return true;
}