public Bundle getCache(@NonNull Uri key){
    try {
        final Bundle bundle = getContentService().getCache(mContext.getPackageName(), key, mContext.getUserId());
        if (bundle != null)
            bundle.setClassLoader(mContext.getClassLoader());
        return bundle;
    } catch (RemoteException e) {
        throw e.rethrowFromSystemServer();
    }
}