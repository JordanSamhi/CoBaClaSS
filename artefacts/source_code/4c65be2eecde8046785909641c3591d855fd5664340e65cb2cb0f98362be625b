public boolean addRoleName(@NonNull String roleName){
    synchronized (mLock) {
        if (!mRoles.containsKey(roleName)) {
            mRoles.put(roleName, new ArraySet<>());
            Slog.i(LOG_TAG, "Added new role: " + roleName);
            scheduleWriteFileLocked();
            return true;
        } else {
            return false;
        }
    }
}