public MediaSession2 build(){
    if (mCallbackExecutor == null) {
        mCallbackExecutor = mContext.getMainExecutor();
    }
    if (mCallback == null) {
        mCallback = new SessionCallback() {
        };
    }
    if (mId == null) {
        mId = "";
    }
    if (mExtras == null) {
        mExtras = Bundle.EMPTY;
    }
    MediaSession2 session2 = new MediaSession2(mContext, mId, mSessionActivity, mCallbackExecutor, mCallback, mExtras);
    try {
        MediaSessionManager manager = (MediaSessionManager) mContext.getSystemService(Context.MEDIA_SESSION_SERVICE);
        manager.notifySession2Created(session2.getToken());
    } catch (Exception e) {
        session2.close();
        throw e;
    }
    return session2;
}