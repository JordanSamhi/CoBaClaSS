public boolean equals(Object rhs){
    if (this == rhs)
        return true;
    if (!(rhs instanceof PnoNetwork)) {
        return false;
    }
    PnoNetwork network = (PnoNetwork) rhs;
    return Arrays.equals(mSsid, network.mSsid) && Arrays.equals(mFrequencies, network.mFrequencies) && mIsHidden == network.mIsHidden;
}