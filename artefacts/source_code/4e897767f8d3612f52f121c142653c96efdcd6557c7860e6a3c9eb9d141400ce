public List<StatusBarIconHolder> getHolderListInViewOrder(){
    ArrayList<StatusBarIconHolder> holders = new ArrayList<>();
    if (mSubSlots != null) {
        for (int i = mSubSlots.size() - 1; i >= 0; i--) {
            holders.add(mSubSlots.get(i));
        }
    }
    if (mHolder != null) {
        holders.add(mHolder);
    }
    return holders;
}