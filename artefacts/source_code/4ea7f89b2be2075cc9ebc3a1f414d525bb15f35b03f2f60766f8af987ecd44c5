public static boolean isTimeBetween(@NonNull LocalTime reference, @NonNull LocalTime start, @NonNull LocalTime end){
    if ((reference.isBefore(start) && reference.isAfter(end) || (reference.isBefore(end) && reference.isBefore(start) && start.isBefore(end)) || (reference.isAfter(end) && reference.isAfter(start)) && start.isBefore(end))) {
        return false;
    } else {
        return true;
    }
}