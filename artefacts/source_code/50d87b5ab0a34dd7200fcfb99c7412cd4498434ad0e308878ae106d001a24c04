public int getCarrierIdFromSimMccMnc(){
    try {
        ITelephony service = getITelephony();
        if (service != null) {
            return service.getCarrierIdFromMccMnc(getSlotIndex(), getSimOperator(), true);
        }
    } catch (RemoteException ex) {
    }
    return UNKNOWN_CARRIER_ID;
}