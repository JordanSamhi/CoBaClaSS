public void testOnChange_whenNewlySetup_schedulesBackup() throws Exception{
    SetupObserver setupObserver = new SetupObserver(mUserBackupManagerService, new Handler());
    mUserBackupManagerService.setSetupComplete(false);
    changeSetupCompleteSettingForUser(true, USER_ID);
    mUserBackupManagerService.setEnabled(true);
    mUserBackupManagerService.enqueueFullBackup("testPackage", 0);
    setupObserver.onChange(true);
    assertThat(mShadowJobScheduler.getPendingJob(getJobIdForUser(KeyValueBackupJob.MIN_JOB_ID, KeyValueBackupJob.MAX_JOB_ID, USER_ID))).isNotNull();
    assertThat(mShadowJobScheduler.getPendingJob(getJobIdForUser(FullBackupJob.MIN_JOB_ID, FullBackupJob.MAX_JOB_ID, USER_ID))).isNotNull();
}