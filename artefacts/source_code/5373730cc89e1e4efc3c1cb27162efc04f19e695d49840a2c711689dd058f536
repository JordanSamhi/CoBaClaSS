public Iterator<KeyCount> iterator(){
    return new Iterator<KeyCount>() {

        private int mIndex = 0;

        @Override
        public boolean hasNext() {
            return mIndex < size();
        }

        @Override
        public KeyCount next() {
            KeyCount kc = new KeyCount(keyAt(mIndex), valueAt(mIndex));
            mIndex++;
            return kc;
        }
    };
}