public boolean removeChild(IBinder token){
    enforceNotSealed();
    if (mChildNodeIds == null || mLeashedChild == null) {
        return false;
    }
    if (!mLeashedChild.equals(token)) {
        return false;
    }
    final int index = mChildNodeIds.indexOf(LEASHED_NODE_ID);
    mLeashedChild = null;
    if (index < 0) {
        return false;
    }
    mChildNodeIds.remove(index);
    return true;
}