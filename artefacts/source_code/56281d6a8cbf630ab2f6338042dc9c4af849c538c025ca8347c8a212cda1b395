public void switchMultiSimConfig(int numOfSims){
    try {
        ITelephony telephony = getITelephony();
        if (telephony != null) {
            telephony.switchMultiSimConfig(numOfSims);
        }
    } catch (RemoteException ex) {
        Rlog.e(TAG, "switchMultiSimConfig RemoteException", ex);
    }
}