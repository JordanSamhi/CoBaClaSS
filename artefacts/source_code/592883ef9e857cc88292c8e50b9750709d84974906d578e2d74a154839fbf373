public void systemBooted(){
    bindKeyguard();
    synchronized (mLock) {
        mSystemBooted = true;
        if (mSystemReady) {
            mKeyguardDelegate.onBootCompleted();
        }
    }
    startedWakingUp(ON_BECAUSE_OF_UNKNOWN);
    finishedWakingUp(ON_BECAUSE_OF_UNKNOWN);
    screenTurningOn(null);
    screenTurnedOn();
}