public void close(){
    synchronized (mLock) {
        if (!mOpen) {
            return;
        }
        if (mOpenCount != 0) {
            throw new IllegalStateException("Failed to close provider used by " + mOpenCount + " ResourcesLoader instances");
        }
        mOpen = false;
    }
    try {
        mApkAssets.close();
    } catch (Throwable ignored) {
    }
}