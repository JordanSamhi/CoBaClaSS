public void updateWifiUsabilityScore(int seqNum, int score, int predictionHorizonSec){
    mContext.enforceCallingOrSelfPermission(android.Manifest.permission.WIFI_UPDATE_USABILITY_STATS_SCORE, "WifiService");
    if (mVerboseLoggingEnabled) {
        mLog.info("updateWifiUsabilityScore uid=% seqNum=% score=% predictionHorizonSec=%").c(Binder.getCallingUid()).c(seqNum).c(score).c(predictionHorizonSec).flush();
    }
    mWifiThreadRunner.post(() -> mClientModeImpl.updateWifiUsabilityScore(seqNum, score, predictionHorizonSec));
}