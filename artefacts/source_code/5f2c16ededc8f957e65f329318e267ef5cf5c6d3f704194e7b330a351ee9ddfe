public boolean sdpSearch(ParcelUuid uuid){
    final IBluetooth service = sService;
    if (service == null) {
        Log.e(TAG, "BT not enabled. Cannot query remote device sdp records");
        return false;
    }
    try {
        return service.sdpSearch(this, uuid);
    } catch (RemoteException e) {
        Log.e(TAG, "", e);
    }
    return false;
}