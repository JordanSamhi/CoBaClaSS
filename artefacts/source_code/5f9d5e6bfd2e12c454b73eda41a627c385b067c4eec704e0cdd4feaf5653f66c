public boolean removeShortcutTargetLocked(@ShortcutType int shortcutType, ComponentName target){
    return getShortcutTargetsLocked(shortcutType).removeIf(name -> {
        ComponentName componentName;
        if (name == null || (componentName = ComponentName.unflattenFromString(name)) == null) {
            return false;
        }
        return componentName.equals(target);
    });
}