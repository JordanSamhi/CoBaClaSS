public Intent getConfigurationIntent(String transportName){
    mContext.enforceCallingOrSelfPermission(android.Manifest.permission.BACKUP, "getConfigurationIntent");
    try {
        Intent intent = mTransportManager.getTransportConfigurationIntent(transportName);
        if (MORE_DEBUG) {
            Slog.d(TAG, addUserIdToLogMessage(mUserId, "getConfigurationIntent() returning intent " + intent));
        }
        return intent;
    } catch (TransportNotRegisteredException e) {
        Slog.e(TAG, addUserIdToLogMessage(mUserId, "Unable to get configuration intent from transport: " + e.getMessage()));
        return null;
    }
}