public String getSmscAddress(){
    String smsc = null;
    try {
        ISms iSms = getISmsService();
        if (iSms != null) {
            smsc = iSms.getSmscAddressFromIccEfForSubscriber(getSubscriptionId(), null);
        }
    } catch (RemoteException ex) {
        throw new RuntimeException(ex);
    }
    return smsc;
}