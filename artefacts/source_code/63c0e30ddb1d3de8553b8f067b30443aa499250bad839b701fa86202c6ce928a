public void scheduleNonStrongBiometricIdleTimeout(int userId){
    if (DEBUG)
        Slog.d(TAG, "scheduleNonStrongBiometricIdleTimeout for userId=" + userId);
    final int argNotUsed = 0;
    mHandler.obtainMessage(MSG_SCHEDULE_NON_STRONG_BIOMETRIC_IDLE_TIMEOUT, userId, argNotUsed).sendToTarget();
}