public void overrideConfig(int subscriptionId, @Nullable PersistableBundle overrideValues, boolean persistent){
    try {
        ICarrierConfigLoader loader = getICarrierConfigLoader();
        if (loader == null) {
            Rlog.w(TAG, "Error setting config for subId " + subscriptionId + " ICarrierConfigLoader is null");
            return;
        }
        loader.overrideConfig(subscriptionId, overrideValues, persistent);
    } catch (RemoteException ex) {
        Rlog.e(TAG, "Error setting config for subId " + subscriptionId + ": " + ex.toString());
    }
}