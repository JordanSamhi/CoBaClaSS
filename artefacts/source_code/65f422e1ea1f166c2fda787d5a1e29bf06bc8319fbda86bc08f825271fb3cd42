public static String toString(int mode){
    switch(mode) {
        case MODE_11A:
            return "MODE_11A";
        case MODE_11B:
            return "MODE_11B";
        case MODE_11G:
            return "MODE_11G";
        case MODE_11N:
            return "MODE_11N";
        case MODE_11AC:
            return "MODE_11AC";
        case MODE_11AX:
            return "MODE_11AX";
        default:
            return "MODE_UNDEFINED";
    }
}