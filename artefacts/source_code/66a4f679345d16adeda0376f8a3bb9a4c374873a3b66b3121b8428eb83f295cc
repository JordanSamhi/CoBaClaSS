public static KeyguardViewMediator newKeyguardViewMediator(Context context, FalsingManager falsingManager, LockPatternUtils lockPatternUtils, BroadcastDispatcher broadcastDispatcher, Lazy<KeyguardViewController> statusBarKeyguardViewManagerLazy, DismissCallbackRegistry dismissCallbackRegistry, KeyguardUpdateMonitor updateMonitor, DumpManager dumpManager, PowerManager powerManager, TrustManager trustManager, @UiBackground Executor uiBgExecutor, DeviceConfigProxy deviceConfig, NavigationModeController navigationModeController){
    return new KeyguardViewMediator(context, falsingManager, lockPatternUtils, broadcastDispatcher, statusBarKeyguardViewManagerLazy, dismissCallbackRegistry, updateMonitor, dumpManager, uiBgExecutor, powerManager, trustManager, deviceConfig, navigationModeController);
}