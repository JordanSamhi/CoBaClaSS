public void setSubscriptionOverride(int subId, @SubscriptionOverrideMask int overrideMask, @SubscriptionOverrideMask int overrideValue, long timeoutMillis, @NonNull String callingPackage){
    try {
        mService.setSubscriptionOverride(subId, overrideMask, overrideValue, timeoutMillis, callingPackage);
    } catch (RemoteException e) {
        throw e.rethrowFromSystemServer();
    }
}