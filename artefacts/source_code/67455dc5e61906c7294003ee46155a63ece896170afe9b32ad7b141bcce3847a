public void close(){
    try {
        mService.releaseSecurityParameterIndex(mResourceId);
    } catch (RemoteException e) {
        throw e.rethrowFromSystemServer();
    } catch (Exception e) {
        Log.e(TAG, "Failed to close " + this + ", Exception=" + e);
    } finally {
        mResourceId = INVALID_RESOURCE_ID;
        mCloseGuard.close();
    }
}