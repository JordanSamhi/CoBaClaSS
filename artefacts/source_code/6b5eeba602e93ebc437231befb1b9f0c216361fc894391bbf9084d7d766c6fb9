public Iterator<Bucket> iterator(){
    return new Iterator<Bucket>() {

        private int mBucketIndex = 0;

        @Override
        public boolean hasNext() {
            return mBucketIndex < mBuckets.size();
        }

        @Override
        public Bucket next() {
            Bucket bucket = getBucketByIndex(mBucketIndex);
            mBucketIndex++;
            return bucket;
        }
    };
}