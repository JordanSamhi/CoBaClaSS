public void dump(DualDumpOutputStream dump, String idName, long id){
    long token = dump.start(idName, id);
    dump.write("connected_to_adb", AdbDebuggingManagerProto.CONNECTED_TO_ADB, mThread != null);
    writeStringIfNotNull(dump, "last_key_received", AdbDebuggingManagerProto.LAST_KEY_RECEVIED, mFingerprints);
    try {
        dump.write("user_keys", AdbDebuggingManagerProto.USER_KEYS, FileUtils.readTextFile(new File("/data/misc/adb/adb_keys"), 0, null));
    } catch (IOException e) {
        Slog.e(TAG, "Cannot read user keys", e);
    }
    try {
        dump.write("system_keys", AdbDebuggingManagerProto.SYSTEM_KEYS, FileUtils.readTextFile(new File("/adb_keys"), 0, null));
    } catch (IOException e) {
        Slog.e(TAG, "Cannot read system keys", e);
    }
    try {
        dump.write("keystore", AdbDebuggingManagerProto.KEYSTORE, FileUtils.readTextFile(getAdbTempKeysFile(), 0, null));
    } catch (IOException e) {
        Slog.e(TAG, "Cannot read keystore: ", e);
    }
    dump.end(token);
}