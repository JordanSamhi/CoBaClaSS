public void updateEF(AdnRecord adn, int ef, int extensionEF, int recordNumber, String pin2, Message response){
    mEf = ef;
    mExtensionEF = extensionEF;
    mRecordNumber = recordNumber;
    mUserResponse = response;
    mPin2 = pin2;
    mFh.getEFLinearRecordSize(ef, getEFPath(ef), obtainMessage(EVENT_EF_LINEAR_RECORD_SIZE_DONE, adn));
}