public UserHandle getDeviceOwnerUser(){
    if (mService != null) {
        try {
            int userId = mService.getDeviceOwnerUserId();
            if (userId != UserHandle.USER_NULL) {
                return UserHandle.of(userId);
            }
        } catch (RemoteException re) {
            throw re.rethrowFromSystemServer();
        }
    }
    return null;
}