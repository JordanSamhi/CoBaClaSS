public void saveLockPin(byte[] pin){
    if (mCurrentSaveLockTask != null && mCurrentSaveLockTask.getStatus() != AsyncTask.Status.FINISHED) {
        Log.e(TAG, "Can't save multiple passwords at once");
        return;
    }
    mCurrentSaveLockTask = new SaveLockAsyncTask(mInitialLockListener, mInitialLockSetupService, LockTypes.PIN);
    mCurrentSaveLockTask.executeOnExecutor(AsyncTask.THREAD_POOL_EXECUTOR, pin);
}