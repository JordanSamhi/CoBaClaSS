public static boolean isValidPhysicalAddress(int address){
    if (address < 0 || address >= 0xFFFF) {
        return false;
    }
    int mask = 0xF000;
    boolean hasZero = false;
    for (int i = 0; i < 4; i++) {
        if ((address & mask) == 0) {
            hasZero = true;
        } else if (hasZero) {
            return false;
        }
        mask >>= 4;
    }
    return true;
}