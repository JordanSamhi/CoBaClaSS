public static WrappedKeyProto.WrappedKey rewrap(SecretKey oldSecondaryKey, SecretKey newSecondaryKey, WrappedKeyProto.WrappedKey tertiaryKey) throws InvalidKeyException, IllegalBlockSizeException, InvalidAlgorithmParameterException, NoSuchAlgorithmException, NoSuchPaddingException{
    return wrap(newSecondaryKey, unwrap(oldSecondaryKey, tertiaryKey));
}