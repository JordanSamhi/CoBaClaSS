public void start(){
    super.start();
    if (DEBUG) {
        log("start");
    }
    if (!isInitialized()) {
        return;
    }
    if (mTaskToken == null) {
        return;
    }
    WindowContainerTransaction wct = new WindowContainerTransaction();
    wct.setHidden(mTaskToken, false);
    WindowOrganizer.applyTransaction(wct);
    if (mListener != null) {
        mListener.onTaskVisibilityChanged(getTaskId(), true);
    }
}