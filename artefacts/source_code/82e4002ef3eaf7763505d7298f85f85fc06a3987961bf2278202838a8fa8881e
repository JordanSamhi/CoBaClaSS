public static float getFinalTranslationZ(View view){
    if (view == null) {
        return 0;
    }
    ValueAnimator zAnimator = getChildTag(view, TAG_ANIMATOR_TRANSLATION_Z);
    if (zAnimator == null) {
        return view.getTranslationZ();
    } else {
        return getChildTag(view, TAG_END_TRANSLATION_Z);
    }
}