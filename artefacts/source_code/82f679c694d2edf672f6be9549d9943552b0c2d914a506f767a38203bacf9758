public static FixedRotationAdjustmentsItem obtain(IBinder token, FixedRotationAdjustments fixedRotationAdjustments){
    FixedRotationAdjustmentsItem instance = ObjectPool.obtain(FixedRotationAdjustmentsItem.class);
    if (instance == null) {
        instance = new FixedRotationAdjustmentsItem();
    }
    instance.mToken = token;
    instance.mFixedRotationAdjustments = fixedRotationAdjustments;
    return instance;
}