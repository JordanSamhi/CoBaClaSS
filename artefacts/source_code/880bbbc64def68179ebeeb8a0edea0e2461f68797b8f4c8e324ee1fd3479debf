public String getCdmaPrlVersion(int subId){
    try {
        ITelephony service = getITelephony();
        if (service != null) {
            return service.getCdmaPrlVersion(subId);
        }
    } catch (RemoteException e) {
        Log.e(TAG, "Error calling ITelephony#getCdmaPrlVersion", e);
    }
    return null;
}