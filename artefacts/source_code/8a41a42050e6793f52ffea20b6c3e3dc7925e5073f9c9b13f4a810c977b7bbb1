public boolean iccCloseLogicalChannel(int subId, int channel){
    try {
        ITelephony telephony = getITelephony();
        if (telephony != null)
            return telephony.iccCloseLogicalChannel(subId, channel);
    } catch (RemoteException ex) {
    } catch (NullPointerException ex) {
    }
    return false;
}