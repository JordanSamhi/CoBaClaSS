public void setMaxBinderCallStats(int maxKeys){
    if (maxKeys <= 0) {
        Slog.w(TAG, "Ignored invalid max value (value must be positive): " + maxKeys);
        return;
    }
    synchronized (mLock) {
        if (maxKeys != mMaxBinderCallStatsCount) {
            mMaxBinderCallStatsCount = maxKeys;
            reset();
        }
    }
}