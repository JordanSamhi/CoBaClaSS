public boolean reconnect(String packageName){
    if (enforceChangePermission(packageName) != MODE_ALLOWED) {
        return false;
    }
    if (!isTargetSdkLessThanQOrPrivileged(packageName, Binder.getCallingPid(), Binder.getCallingUid())) {
        mLog.info("reconnect not allowed for uid=%").c(Binder.getCallingUid()).flush();
        return false;
    }
    mLog.info("reconnect uid=%").c(Binder.getCallingUid()).flush();
    mClientModeImpl.reconnectCommand(new WorkSource(Binder.getCallingUid()));
    return true;
}