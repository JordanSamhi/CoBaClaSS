public static int getNameSourcePriority(@SimDisplayNameSource int nameSource){
    int index = Arrays.asList(SubscriptionManager.NAME_SOURCE_CARRIER_ID, SubscriptionManager.NAME_SOURCE_SIM_PNN, SubscriptionManager.NAME_SOURCE_SIM_SPN, SubscriptionManager.NAME_SOURCE_CARRIER, SubscriptionManager.NAME_SOURCE_USER_INPUT).indexOf(nameSource);
    return (index < 0) ? 0 : index;
}