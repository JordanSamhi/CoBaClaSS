public static String rilServiceStateToString(int serviceState){
    switch(serviceState) {
        case STATE_IN_SERVICE:
            return "IN_SERVICE";
        case STATE_OUT_OF_SERVICE:
            return "OUT_OF_SERVICE";
        case STATE_EMERGENCY_ONLY:
            return "EMERGENCY_ONLY";
        case STATE_POWER_OFF:
            return "POWER_OFF";
        default:
            return "UNKNOWN";
    }
}