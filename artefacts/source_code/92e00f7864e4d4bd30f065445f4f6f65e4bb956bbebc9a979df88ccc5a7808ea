public Map<String, Map<Integer, List<ScanResult>>> getAllMatchingPasspointProfilesForScanResults(List<ScanResult> scanResults){
    if (!isSettingsOrSuw(Binder.getCallingPid(), Binder.getCallingUid())) {
        throw new SecurityException(TAG + ": Permission denied");
    }
    if (mVerboseLoggingEnabled) {
        mLog.info("getMatchingPasspointConfigurations uid=%").c(Binder.getCallingUid()).flush();
    }
    if (!ScanResultUtil.validateScanResultList(scanResults)) {
        Log.e(TAG, "Attempt to retrieve passpoint with invalid scanResult List");
        return Collections.emptyMap();
    }
    return mWifiThreadRunner.call(() -> mPasspointManager.getAllMatchingPasspointProfilesForScanResults(scanResults), Collections.emptyMap());
}