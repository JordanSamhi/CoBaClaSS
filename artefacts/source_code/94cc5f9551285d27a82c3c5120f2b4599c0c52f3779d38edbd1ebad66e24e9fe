public int getRilVoiceRadioTechnology(){
    NetworkRegistrationInfo wwanRegInfo = getNetworkRegistrationInfo(NetworkRegistrationInfo.DOMAIN_CS, AccessNetworkConstants.TRANSPORT_TYPE_WWAN);
    if (wwanRegInfo != null) {
        return networkTypeToRilRadioTechnology(wwanRegInfo.getAccessNetworkTechnology());
    }
    return RIL_RADIO_TECHNOLOGY_UNKNOWN;
}