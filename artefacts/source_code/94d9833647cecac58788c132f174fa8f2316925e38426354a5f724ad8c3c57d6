public boolean allowsManagedPrivateNotificationsInPublic(){
    synchronized (mLock) {
        for (int i = mCurrentManagedProfiles.size() - 1; i >= 0; i--) {
            if (!userAllowsPrivateNotificationsInPublic(mCurrentManagedProfiles.valueAt(i).id)) {
                return false;
            }
        }
    }
    return true;
}