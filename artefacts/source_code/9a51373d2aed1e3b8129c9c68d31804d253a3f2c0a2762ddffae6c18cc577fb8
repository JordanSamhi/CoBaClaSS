public static String frequencyRangeToString(@FrequencyRange int range){
    switch(range) {
        case FREQUENCY_RANGE_UNKNOWN:
            return "UNKNOWN";
        case FREQUENCY_RANGE_LOW:
            return "LOW";
        case FREQUENCY_RANGE_MID:
            return "MID";
        case FREQUENCY_RANGE_HIGH:
            return "HIGH";
        case FREQUENCY_RANGE_MMWAVE:
            return "MMWAVE";
        default:
            return Integer.toString(range);
    }
}