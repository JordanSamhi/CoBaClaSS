public void logLocationApiUsage(int usageType, int apiInUse, String providerName){
    try {
        if (hitApiUsageLogCap()) {
            return;
        }
        FrameworkStatsLog.write(FrameworkStatsLog.LOCATION_MANAGER_API_USAGE_REPORTED, usageType, apiInUse, null, bucketizeProvider(providerName), LocationStatsEnums.QUALITY_UNKNOWN, LocationStatsEnums.INTERVAL_UNKNOWN, LocationStatsEnums.DISTANCE_UNKNOWN, 0, LocationStatsEnums.EXPIRATION_UNKNOWN, getCallbackType(apiInUse, true, true), 0, LocationStatsEnums.IMPORTANCE_UNKNOWN);
    } catch (Exception e) {
        Log.w(TAG, "Failed to log API usage to statsd.", e);
    }
}