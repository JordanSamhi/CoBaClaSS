public void dump(PrintWriter pw){
    pw.print(" state=");
    pw.println(mState);
    pw.print(" wakeLockHeldForCurrentState=");
    pw.println(mWakeLockHeldForCurrentState);
    pw.print(" wakeLock=");
    pw.println(mWakeLock);
    pw.println("Parts:");
    for (Part p : mParts) {
        p.dump(pw);
    }
}