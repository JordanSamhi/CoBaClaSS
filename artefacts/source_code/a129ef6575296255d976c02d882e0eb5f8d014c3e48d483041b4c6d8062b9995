public static boolean isTetheringRequestEquals(final TetheringRequestParcel request, final TetheringRequestParcel otherRequest){
    if (request == otherRequest)
        return true;
    return request != null && otherRequest != null && request.tetheringType == otherRequest.tetheringType && Objects.equals(request.localIPv4Address, otherRequest.localIPv4Address) && Objects.equals(request.staticClientAddress, otherRequest.staticClientAddress) && request.exemptFromEntitlementCheck == otherRequest.exemptFromEntitlementCheck && request.showProvisioningUi == otherRequest.showProvisioningUi;
}