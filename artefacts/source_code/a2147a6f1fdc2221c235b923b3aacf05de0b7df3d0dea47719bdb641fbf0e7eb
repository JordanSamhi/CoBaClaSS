public static boolean shouldReportFullScanResultForSettings(ChannelHelper channelHelper, ScanResult result, int bucketsScanned, ScanSettings settings, int scheduledBucket){
    if (isBucketMaybeScanned(scheduledBucket, bucketsScanned)) {
        return channelHelper.settingsContainChannel(settings, result.frequency);
    } else {
        return false;
    }
}