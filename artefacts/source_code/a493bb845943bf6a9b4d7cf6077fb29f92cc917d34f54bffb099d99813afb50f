public long getLong(String key, long defaultValue){
    unparcel();
    Object o = mMap.get(key);
    if (o == null) {
        return defaultValue;
    }
    try {
        return (Long) o;
    } catch (ClassCastException e) {
        typeWarning(key, o, "Long", defaultValue, e);
        return defaultValue;
    }
}