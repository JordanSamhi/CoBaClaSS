public static PlmnActRecord[] getRecords(byte[] recordBytes){
    if (recordBytes == null || recordBytes.length == 0 || recordBytes.length % ENCODED_LENGTH != 0) {
        Rlog.e(LOG_TAG, "Malformed PlmnActRecord, bytes: " + ((recordBytes != null) ? Arrays.toString(recordBytes) : null));
        return null;
    }
    int numRecords = recordBytes.length / ENCODED_LENGTH;
    if (VDBG)
        Rlog.v(LOG_TAG, "Extracting Logs, count=" + numRecords);
    PlmnActRecord[] records = new PlmnActRecord[numRecords];
    for (int i = 0; i < numRecords; i++) {
        records[i] = new PlmnActRecord(recordBytes, i * ENCODED_LENGTH);
    }
    return records;
}