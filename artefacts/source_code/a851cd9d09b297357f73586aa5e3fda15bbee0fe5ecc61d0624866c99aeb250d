public void reset(){
    mLastTxBytes = 0;
    mLastRxBytes = 0;
    mLastFrequency = -1;
    mLastBssid = null;
    mDataStallStartTimeMs = -1;
    mDataStallTx = false;
    mDataStallRx = false;
    mIsThroughputSufficient = true;
    mTxTputKbps = INVALID_THROUGHPUT;
    mRxTputKbps = INVALID_THROUGHPUT;
}