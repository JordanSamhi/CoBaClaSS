public void notifyLaunchLocationShown(@NonNull String launchLocation, @NonNull List<AppTargetId> targetIds){
    if (mIsClosed.get()) {
        throw new IllegalStateException("This client has already been destroyed.");
    }
    try {
        mPredictionManager.notifyLaunchLocationShown(mSessionId, launchLocation, new ParceledListSlice<>(targetIds));
    } catch (RemoteException e) {
        Log.e(TAG, "Failed to notify location shown event", e);
        e.rethrowAsRuntimeException();
    }
}