public void notify(int eventCode, @Nullable V info){
    Object[] listeners;
    synchronized (mListeners) {
        if (mListeners.size() == 0) {
            return;
        }
        listeners = mListeners.values().toArray();
    }
    final Long identity = mClearCallingIdentity ? Binder.clearCallingIdentity() : null;
    try {
        for (Object object : listeners) {
            final ListenerWithCancellation<V> listener = (ListenerWithCancellation<V>) object;
            listener.onEvent(eventCode, info);
        }
    } finally {
        if (identity != null) {
            Binder.restoreCallingIdentity(identity);
        }
    }
}