public boolean toShortStringTypesOnly(StringBuilder b){
    boolean first = true;
    final int size = mMimeTypes.size();
    for (int i = 0; i < size; i++) {
        if (!first) {
            b.append(' ');
        }
        first = false;
        b.append(mMimeTypes.get(i));
    }
    return !first;
}