public boolean isHighPriority(ListEntry entry){
    if (entry == null) {
        return false;
    }
    final NotificationEntry notifEntry = entry.getRepresentativeEntry();
    if (notifEntry == null) {
        return false;
    }
    return notifEntry.getRanking().getImportance() >= NotificationManager.IMPORTANCE_DEFAULT || hasHighPriorityCharacteristics(notifEntry) || hasHighPriorityChild(entry);
}