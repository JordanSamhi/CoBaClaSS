public void removeHdmiCecVolumeControlFeatureListener(HdmiCecVolumeControlFeatureListener listener){
    if (mService == null) {
        Log.e(TAG, "HdmiControlService is not available");
        return;
    }
    IHdmiCecVolumeControlFeatureListener wrappedListener = mHdmiCecVolumeControlFeatureListeners.remove(listener);
    if (wrappedListener == null) {
        Log.e(TAG, "tried to remove not-registered listener");
        return;
    }
    try {
        mService.removeHdmiCecVolumeControlFeatureListener(wrappedListener);
    } catch (RemoteException e) {
        throw e.rethrowFromSystemServer();
    }
}