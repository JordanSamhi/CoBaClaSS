public int[] getActiveSubscriptionIdList(boolean visibleOnly){
    try {
        ISub iSub = TelephonyManager.getSubscriptionService();
        if (iSub != null) {
            int[] subId = iSub.getActiveSubIdList(visibleOnly);
            if (subId != null)
                return subId;
        }
    } catch (RemoteException ex) {
    }
    return new int[0];
}