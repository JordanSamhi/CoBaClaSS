public static int convertCallState(PhoneConstants.State state){
    switch(state) {
        case RINGING:
            return TelephonyManager.CALL_STATE_RINGING;
        case OFFHOOK:
            return TelephonyManager.CALL_STATE_OFFHOOK;
        default:
            return TelephonyManager.CALL_STATE_IDLE;
    }
}