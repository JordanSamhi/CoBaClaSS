public static float getFinalTranslationX(View view){
    if (view == null) {
        return 0;
    }
    ValueAnimator xAnimator = getChildTag(view, TAG_ANIMATOR_TRANSLATION_X);
    if (xAnimator == null) {
        return view.getTranslationX();
    } else {
        return getChildTag(view, TAG_END_TRANSLATION_X);
    }
}