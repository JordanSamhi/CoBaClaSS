public boolean isSameCell(@Nullable CellIdentity ci){
    if (ci == null)
        return false;
    if (this.getClass() != ci.getClass())
        return false;
    if (this.getGlobalCellId() == null || ci.getGlobalCellId() == null)
        return false;
    return TextUtils.equals(this.getGlobalCellId(), ci.getGlobalCellId());
}