public boolean init(final Uri contentUri, final String[] projection, final String selection, final String[] selectionArgs, final String sortOrder, Uri contentUriForRawContactsEntity){
    if (!ContactsContract.AUTHORITY.equals(contentUri.getAuthority())) {
        if (DEBUG)
            Log.d(LOG_TAG, "Unexpected contentUri: " + contentUri);
        mErrorReason = FAILURE_REASON_UNSUPPORTED_URI;
        return false;
    }
    if (!initInterFirstPart(contentUriForRawContactsEntity)) {
        return false;
    }
    if (!initInterCursorCreationPart(contentUri, projection, selection, selectionArgs, sortOrder)) {
        return false;
    }
    if (!initInterMainPart()) {
        return false;
    }
    return initInterLastPart();
}