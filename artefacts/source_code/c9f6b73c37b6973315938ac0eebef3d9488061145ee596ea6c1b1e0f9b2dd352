public void setLayerTracingFlags(int flags){
    mAtmInternal.enforceCallerIsRecentsOrHasPermission(android.Manifest.permission.DUMP, "setLayerTracingFlags");
    long token = Binder.clearCallingIdentity();
    try {
        Parcel data = null;
        try {
            IBinder sf = ServiceManager.getService("SurfaceFlinger");
            if (sf != null) {
                data = Parcel.obtain();
                data.writeInterfaceToken("android.ui.ISurfaceComposer");
                data.writeInt(flags);
                sf.transact(1033, data, null, 0);
            }
        } catch (RemoteException e) {
            Slog.e(TAG, "Failed to set layer tracing flags");
        } finally {
            if (data != null) {
                data.recycle();
            }
        }
    } finally {
        Binder.restoreCallingIdentity(token);
    }
}