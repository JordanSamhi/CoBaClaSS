public boolean equals(Object obj){
    if (!(obj instanceof UsbAlsaDevice)) {
        return false;
    }
    UsbAlsaDevice other = (UsbAlsaDevice) obj;
    return (mCardNum == other.mCardNum && mDeviceNum == other.mDeviceNum && mHasOutput == other.mHasOutput && mHasInput == other.mHasInput && mIsInputHeadset == other.mIsInputHeadset && mIsOutputHeadset == other.mIsOutputHeadset);
}