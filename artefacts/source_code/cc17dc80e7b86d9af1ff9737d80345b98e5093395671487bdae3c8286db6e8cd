public void requestShowFillUi(AutofillId id, int width, int height, IAutofillWindowPresenter presenter){
    synchronized (mLock) {
        if (mDestroyed) {
            Slog.w(TAG, "Call to Session#requestShowFillUi() rejected - session: " + id + " destroyed");
            return;
        }
        if (id.equals(mCurrentViewId)) {
            try {
                final ViewState view = mViewStates.get(id);
                mClient.requestShowFillUi(this.id, id, width, height, view.getVirtualBounds(), presenter);
            } catch (RemoteException e) {
                Slog.e(TAG, "Error requesting to show fill UI", e);
            }
        } else {
            if (sDebug) {
                Slog.d(TAG, "Do not show full UI on " + id + " as it is not the current view (" + mCurrentViewId + ") anymore");
            }
        }
    }
}