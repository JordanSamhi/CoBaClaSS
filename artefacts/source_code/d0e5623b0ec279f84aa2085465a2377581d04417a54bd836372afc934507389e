public final void setSupportedAudioRoutes(int supportedAudioRoutes){
    if ((supportedAudioRoutes & (CallAudioState.ROUTE_EARPIECE | CallAudioState.ROUTE_SPEAKER)) == 0) {
        throw new IllegalArgumentException("supported audio routes must include either speaker or earpiece");
    }
    if (mSupportedAudioRoutes != supportedAudioRoutes) {
        mSupportedAudioRoutes = supportedAudioRoutes;
        for (Listener l : mListeners) {
            l.onSupportedAudioRoutesChanged(this, mSupportedAudioRoutes);
        }
    }
}