public void addOnWifiUsabilityListener(IBinder binder, IOnWifiUsabilityStatsListener listener, int listenerIdentifier){
    if (!mOnWifiUsabilityListeners.add(binder, listener, listenerIdentifier)) {
        Log.e(TAG, "Failed to add listener");
        return;
    }
    if (DBG) {
        Log.v(TAG, "Adding listener. Num listeners: " + mOnWifiUsabilityListeners.getNumCallbacks());
    }
}