public void unregisterSuggestionConnectionStatusListener(int listenerIdentifier, String packageName){
    enforceAccessPermission();
    if (mVerboseLoggingEnabled) {
        mLog.info("unregisterSuggestionConnectionStatusListener uid=%").c(Binder.getCallingUid()).flush();
    }
    mWifiThreadRunner.post(() -> mWifiNetworkSuggestionsManager.unregisterSuggestionConnectionStatusListener(listenerIdentifier, packageName));
}