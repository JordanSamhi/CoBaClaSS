public void writeRilAnswer(int phoneId, int rilSerial){
    InProgressCallSession callSession = mInProgressCallSessions.get(phoneId);
    if (callSession == null) {
        Rlog.e(TAG, "writeRilAnswer: Call session is missing");
    } else {
        callSession.addEvent(new CallSessionEventBuilder(TelephonyCallSession.Event.Type.RIL_REQUEST).setRilRequest(TelephonyCallSession.Event.RilRequest.RIL_REQUEST_ANSWER).setRilRequestId(rilSerial));
    }
}