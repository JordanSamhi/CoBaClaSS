public int getSubscriptionId(@NonNull PhoneAccountHandle phoneAccountHandle){
    int retval = SubscriptionManager.INVALID_SUBSCRIPTION_ID;
    try {
        ITelephony service = getITelephony();
        if (service != null) {
            retval = service.getSubIdForPhoneAccountHandle(phoneAccountHandle, mContext.getOpPackageName(), mContext.getAttributionTag());
        }
    } catch (RemoteException ex) {
        Log.e(TAG, "getSubscriptionId RemoteException", ex);
        ex.rethrowAsRuntimeException();
    }
    return retval;
}