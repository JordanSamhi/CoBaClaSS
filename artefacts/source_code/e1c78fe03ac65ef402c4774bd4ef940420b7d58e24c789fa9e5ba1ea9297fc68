public static C checkCollectionElementsNotNull(final C value, final String valueName){
    if (value == null) {
        throw new NullPointerException(valueName + " must not be null");
    }
    long ctr = 0;
    for (T elem : value) {
        if (elem == null) {
            throw new NullPointerException(String.format("%s[%d] must not be null", valueName, ctr));
        }
        ++ctr;
    }
    return value;
}