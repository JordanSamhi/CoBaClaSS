public void placeCall(@NonNull ICallRedirectionAdapter adapter, @NonNull Uri handle, @NonNull PhoneAccountHandle initialPhoneAccount, boolean allowInteractiveResponse){
    SomeArgs args = SomeArgs.obtain();
    args.arg1 = adapter;
    args.arg2 = handle;
    args.arg3 = initialPhoneAccount;
    args.arg4 = allowInteractiveResponse;
    mHandler.obtainMessage(MSG_PLACE_CALL, args).sendToTarget();
}