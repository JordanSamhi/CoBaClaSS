public boolean destroyCeSnapshotsNotSpecified(@UserIdInt int userId, int[] retainSnapshotIds) throws InstallerException{
    if (!checkBeforeRemote())
        return false;
    try {
        mInstalld.destroyCeSnapshotsNotSpecified(null, userId, retainSnapshotIds);
        return true;
    } catch (Exception e) {
        throw InstallerException.from(e);
    }
}