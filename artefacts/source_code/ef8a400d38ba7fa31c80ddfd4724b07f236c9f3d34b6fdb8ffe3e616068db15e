public void setFrameRate(@FloatRange(from = 0.0) float frameRate, @FrameRateCompatibility int compatibility){
    synchronized (mLock) {
        checkNotReleasedLocked();
        int error = nativeSetFrameRate(mNativeObject, frameRate, compatibility);
        if (error == -EINVAL) {
            throw new IllegalArgumentException("Invalid argument to Surface.setFrameRate()");
        } else if (error != 0) {
            throw new RuntimeException("Failed to set frame rate on Surface");
        }
    }
}