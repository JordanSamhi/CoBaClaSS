public void startProtoLog(@Nullable PrintWriter pw){
    if (isProtoEnabled()) {
        return;
    }
    synchronized (mProtoLogEnabledLock) {
        logAndPrintln(pw, "Start logging to " + mLogFile + ".");
        mBuffer.resetBuffer();
        mProtoLogEnabled = true;
        mProtoLogEnabledLockFree = true;
    }
    sCacheUpdater.run();
}