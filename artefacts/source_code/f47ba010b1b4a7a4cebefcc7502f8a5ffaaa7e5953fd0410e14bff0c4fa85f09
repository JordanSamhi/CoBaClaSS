public void injectGnssMeasurementCorrections(GnssMeasurementCorrections measurementCorrections, String packageName){
    mContext.enforceCallingPermission(Manifest.permission.LOCATION_HARDWARE, null);
    mContext.enforceCallingPermission(Manifest.permission.ACCESS_FINE_LOCATION, null);
    mGnssMeasurementCorrectionsProvider.injectGnssMeasurementCorrections(measurementCorrections);
}