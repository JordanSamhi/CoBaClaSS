public static int[] getSubId(int slotIndex){
    if (!isValidSlotIndex(slotIndex)) {
        logd("[getSubId]- fail");
        return null;
    }
    int[] subId = null;
    try {
        ISub iSub = TelephonyManager.getSubscriptionService();
        if (iSub != null) {
            subId = iSub.getSubId(slotIndex);
        }
    } catch (RemoteException ex) {
    }
    return subId;
}