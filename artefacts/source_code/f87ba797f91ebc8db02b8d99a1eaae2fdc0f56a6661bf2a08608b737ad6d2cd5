public void reset(){
    mProvider = null;
    mTime = 0;
    mElapsedRealtimeNanos = 0;
    mElapsedRealtimeUncertaintyNanos = 0.0;
    mFieldsMask = 0;
    mLatitude = 0;
    mLongitude = 0;
    mAltitude = 0;
    mSpeed = 0;
    mBearing = 0;
    mHorizontalAccuracyMeters = 0;
    mVerticalAccuracyMeters = 0;
    mSpeedAccuracyMetersPerSecond = 0;
    mBearingAccuracyDegrees = 0;
    mExtras = null;
}