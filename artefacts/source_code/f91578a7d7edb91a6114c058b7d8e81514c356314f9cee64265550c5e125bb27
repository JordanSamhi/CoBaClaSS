public List<WifiConfiguration> getWifiConfigForMatchedNetworkSuggestionsSharedWithUser(List<ScanResult> scanResults){
    if (!isSettingsOrSuw(Binder.getCallingPid(), Binder.getCallingUid())) {
        throw new SecurityException(TAG + ": Permission denied");
    }
    if (mVerboseLoggingEnabled) {
        mLog.info("getWifiConfigsForMatchedNetworkSuggestions uid=%").c(Binder.getCallingUid()).flush();
    }
    if (!ScanResultUtil.validateScanResultList(scanResults)) {
        Log.e(TAG, "Attempt to retrieve WifiConfiguration with invalid scanResult List");
        return new ArrayList<>();
    }
    return mWifiThreadRunner.call(() -> mWifiNetworkSuggestionsManager.getWifiConfigForMatchedNetworkSuggestionsSharedWithUser(scanResults), Collections.emptyList());
}