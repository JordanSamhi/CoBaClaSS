public void freeUnderlyingResources(){
    int spi = mSpi.getSpi();
    try {
        mSrvConfig.getNetdInstance().ipSecDeleteSecurityAssociation(uid, mConfig.getSourceAddress(), mConfig.getDestinationAddress(), spi, mConfig.getMarkValue(), mConfig.getMarkMask(), mConfig.getXfrmInterfaceId());
    } catch (RemoteException | ServiceSpecificException e) {
        Log.e(TAG, "Failed to delete SA with ID: " + mResourceId, e);
    }
    getResourceTracker().give();
}