public boolean isInterestingToUserLocked(){
    if (mWindowProcessController.isInterestingToUser()) {
        return true;
    }
    final int servicesSize = mServices.size();
    for (int i = 0; i < servicesSize; i++) {
        ServiceRecord r = mServices.valueAt(i);
        if (r.isForeground) {
            return true;
        }
    }
    return false;
}