public void setConnectionMaxSupportedLinkSpeedMbps(int maxSupportedTxLinkSpeedMbps, int maxSupportedRxLinkSpeedMbps){
    synchronized (mLock) {
        if (mCurrentConnectionEvent != null) {
            mCurrentConnectionEvent.mRouterFingerPrint.setMaxSupportedLinkSpeedMbps(maxSupportedTxLinkSpeedMbps, maxSupportedRxLinkSpeedMbps);
        }
    }
}