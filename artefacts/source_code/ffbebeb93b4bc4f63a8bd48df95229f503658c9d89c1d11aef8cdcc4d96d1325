public void snapBubbleBack(View bubbleView, float velX, float velY){
    final int index = mLayout.indexOfChild(bubbleView);
    animationForChildAtIndex(index).position(getBubbleLeft(index), getExpandedY()).withPositionStartVelocities(velX, velY).start(() -> bubbleView.setTranslationZ(0f));
    mMagnetizedBubbleDraggingOut = null;
    updateBubblePositions();
}