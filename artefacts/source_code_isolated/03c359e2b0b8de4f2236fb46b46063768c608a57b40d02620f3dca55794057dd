public boolean equals(Object obj){
    if (obj instanceof Person) {
        final Person other = (Person) obj;
        return Objects.equals(mName, other.mName) && (mIcon == null ? other.mIcon == null : (other.mIcon != null && mIcon.sameAs(other.mIcon))) && Objects.equals(mUri, other.mUri) && Objects.equals(mKey, other.mKey) && mIsBot == other.mIsBot && mIsImportant == other.mIsImportant;
    }
    return false;
}