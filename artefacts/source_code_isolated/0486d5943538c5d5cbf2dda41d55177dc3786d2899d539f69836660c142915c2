public ParcelFileDescriptor getControlFd(long usbFunction){
    FileDescriptor fd = mControlFds.get(usbFunction);
    if (fd == null) {
        return null;
    }
    try {
        return ParcelFileDescriptor.dup(fd);
    } catch (IOException e) {
        Slog.e(TAG, "Could not dup fd for " + usbFunction);
        return null;
    }
}