public void setTintList(ColorStateList tint){
    final ChildDrawable[] array = mLayerState.mChildren;
    final int N = mLayerState.N_CHILDREN;
    for (int i = 0; i < N; i++) {
        final Drawable dr = array[i].mDrawable;
        if (dr != null) {
            dr.setTintList(tint);
        }
    }
}