public static NitzSignalInputFilterPredicate create(@NonNull Context context, @NonNull DeviceState deviceState){
    Objects.requireNonNull(context);
    Objects.requireNonNull(deviceState);
    TrivalentPredicate[] components = new TrivalentPredicate[] { createIgnoreNitzPropertyCheck(deviceState), createBogusElapsedRealtimeCheck(context, deviceState), createNoOldSignalCheck(), createRateLimitCheck(deviceState) };
    return new NitzSignalInputFilterPredicateImpl(components);
}