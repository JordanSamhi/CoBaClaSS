public String getIsimImpi(int subId){
    return callPhoneMethodForSubIdWithPrivilegedCheck(subId, "getIsimImpi", (phone) -> {
        IsimRecords isim = phone.getIsimRecords();
        if (isim != null) {
            return isim.getIsimImpi();
        } else {
            return null;
        }
    });
}