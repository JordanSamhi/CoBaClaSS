public String substring(int beginIndex){
    if (beginIndex < 0) {
        throw new StringIndexOutOfBoundsException(this, beginIndex);
    }
    int subLen = length() - beginIndex;
    if (subLen < 0) {
        throw new StringIndexOutOfBoundsException(this, beginIndex);
    }
    return (beginIndex == 0) ? this : fastSubstring(beginIndex, subLen);
}