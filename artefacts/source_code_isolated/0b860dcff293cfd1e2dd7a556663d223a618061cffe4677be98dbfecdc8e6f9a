public File getTempFileRootDirectory(){
    SharedPreferences prefs = mContext.getSharedPreferences(MbmsTempFileProvider.TEMP_FILE_ROOT_PREF_FILE_NAME, 0);
    String path = prefs.getString(MbmsTempFileProvider.TEMP_FILE_ROOT_PREF_NAME, null);
    if (path != null) {
        return new File(path);
    }
    return null;
}