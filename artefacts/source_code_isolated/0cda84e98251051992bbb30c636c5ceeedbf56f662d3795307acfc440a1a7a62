public void handleCdmaScpMessage(int slotIndex, List<CdmaSmsCbProgramData> smsCbProgramData, String originatingAddress, RemoteCallback callback){
    Consumer<Bundle> consumer = bundle -> {
        callback.sendResult(bundle);
    };
    CellBroadcastService.this.onCdmaScpMessage(slotIndex, smsCbProgramData, originatingAddress, consumer);
}