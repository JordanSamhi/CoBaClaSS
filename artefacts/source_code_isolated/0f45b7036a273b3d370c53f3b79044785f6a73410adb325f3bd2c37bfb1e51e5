public boolean isCapable(@MmTelFeature.MmTelCapabilities.MmTelCapability int capability, @ImsRegistrationImplBase.ImsRegistrationTech int imsRegTech){
    ITelephony iTelephony = getITelephony();
    if (iTelephony == null) {
        throw new RuntimeException("Could not find Telephony Service.");
    }
    try {
        return iTelephony.isCapable(mSubId, capability, imsRegTech);
    } catch (RemoteException e) {
        throw e.rethrowAsRuntimeException();
    }
}