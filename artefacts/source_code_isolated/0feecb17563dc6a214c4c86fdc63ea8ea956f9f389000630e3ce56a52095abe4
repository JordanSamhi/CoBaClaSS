public String toString(){
    return new StringBuilder(TAG).append(":{ mLac=").append(mLac).append(" mCid=").append(mCid).append(" mArfcn=").append(mArfcn).append(" mBsic=").append("0x").append(Integer.toHexString(mBsic)).append(" mMcc=").append(mMccStr).append(" mMnc=").append(mMncStr).append(" mAlphaLong=").append(mAlphaLong).append(" mAlphaShort=").append(mAlphaShort).append(" mAdditionalPlmns=").append(mAdditionalPlmns).append("}").toString();
}