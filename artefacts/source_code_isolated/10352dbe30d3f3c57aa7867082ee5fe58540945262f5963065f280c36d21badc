public void requestProfile(String cardId, String iccid, @CallbackExecutor Executor executor, ResultCallback<EuiccProfileInfo> callback){
    try {
        getIEuiccCardController().getProfile(mContext.getOpPackageName(), cardId, iccid, new IGetProfileCallback.Stub() {

            @Override
            public void onComplete(int resultCode, EuiccProfileInfo profile) {
                final long token = Binder.clearCallingIdentity();
                try {
                    executor.execute(() -> callback.onComplete(resultCode, profile));
                } finally {
                    Binder.restoreCallingIdentity(token);
                }
            }
        });
    } catch (RemoteException e) {
        Log.e(TAG, "Error calling getProfile", e);
        throw e.rethrowFromSystemServer();
    }
}