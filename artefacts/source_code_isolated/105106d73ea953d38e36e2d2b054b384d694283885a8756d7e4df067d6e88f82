public boolean equals(Object o){
    if (this == o)
        return true;
    if (o == null || !(o instanceof AudioPlaybackConfiguration))
        return false;
    AudioPlaybackConfiguration that = (AudioPlaybackConfiguration) o;
    return ((mPlayerIId == that.mPlayerIId) && (mPlayerType == that.mPlayerType) && (mClientUid == that.mClientUid) && (mClientPid == that.mClientPid));
}