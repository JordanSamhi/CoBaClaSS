public void dump(PrintWriter pw, String prefix){
    synchronized (mCallbacks) {
        pw.print(prefix);
        pw.print("callbacks: ");
        pw.println(mCallbacks.size());
        pw.print(prefix);
        pw.print("killed: ");
        pw.println(mKilled);
        pw.print(prefix);
        pw.print("broadcasts count: ");
        pw.println(mBroadcastCount);
    }
}