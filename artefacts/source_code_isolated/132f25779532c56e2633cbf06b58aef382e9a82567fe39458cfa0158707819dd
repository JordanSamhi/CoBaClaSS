public boolean setPermittedCrossProfileNotificationListeners(@NonNull ComponentName admin, @Nullable List<String> packageList){
    throwIfParentInstance("setPermittedCrossProfileNotificationListeners");
    if (mService != null) {
        try {
            return mService.setPermittedCrossProfileNotificationListeners(admin, packageList);
        } catch (RemoteException e) {
            throw e.rethrowFromSystemServer();
        }
    }
    return false;
}