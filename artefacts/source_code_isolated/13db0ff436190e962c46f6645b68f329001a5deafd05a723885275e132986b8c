public boolean equals(Object obj){
    if (this == obj)
        return true;
    if (obj == null)
        return false;
    if (getClass() != obj.getClass())
        return false;
    final LocusId other = (LocusId) obj;
    if (mId == null) {
        if (other.mId != null)
            return false;
    } else {
        if (!mId.equals(other.mId))
            return false;
    }
    return true;
}