public final View[] autofillClientFindViewsByAutofillIdTraversal(@NonNull AutofillId[] autofillId){
    final View[] views = new View[autofillId.length];
    final ArrayList<ViewRootImpl> roots = WindowManagerGlobal.getInstance().getRootViews(getActivityToken());
    for (int rootNum = 0; rootNum < roots.size(); rootNum++) {
        final View rootView = roots.get(rootNum).getView();
        if (rootView != null) {
            final int viewCount = autofillId.length;
            for (int viewNum = 0; viewNum < viewCount; viewNum++) {
                if (views[viewNum] == null) {
                    views[viewNum] = rootView.findViewByAutofillIdTraversal(autofillId[viewNum].getViewId());
                }
            }
        }
    }
    return views;
}