public boolean equals(@Nullable Object obj){
    if (this == obj) {
        return true;
    }
    if (obj == null) {
        return false;
    }
    if (getClass() != obj.getClass()) {
        return false;
    }
    AmbientBrightnessDayStats other = (AmbientBrightnessDayStats) obj;
    return mLocalDate.equals(other.mLocalDate) && Arrays.equals(mBucketBoundaries, other.mBucketBoundaries) && Arrays.equals(mStats, other.mStats);
}