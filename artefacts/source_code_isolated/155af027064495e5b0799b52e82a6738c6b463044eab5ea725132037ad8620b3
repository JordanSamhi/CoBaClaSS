public String getConfigString(int item) throws ImsException{
    String ret = "Unknown";
    try {
        ret = miConfig.getConfigString(item);
    } catch (RemoteException e) {
        throw new ImsException("getConfigString()", e, ImsReasonInfo.CODE_LOCAL_SERVICE_UNAVAILABLE);
    }
    if (DBG)
        Rlog.d(TAG, "getConfigString(): item = " + item + ", ret =" + ret);
    return ret;
}