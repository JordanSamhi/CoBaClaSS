public void onPendingSaveUi(int operation, IBinder token){
    if (sVerbose)
        Log.v(TAG, "onPendingSaveUi(" + operation + "): " + token);
    synchronized (mLock) {
        try {
            mService.onPendingSaveUi(operation, token);
        } catch (RemoteException e) {
            Log.e(TAG, "Error in onPendingSaveUi: ", e);
        }
    }
}