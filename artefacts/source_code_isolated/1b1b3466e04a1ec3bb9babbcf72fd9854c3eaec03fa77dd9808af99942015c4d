public static void detectBlockingIpcs(Runnable runnable){
    if (STRICT_MODE_ENABLED && sBlockingIpcs.empty()) {
        synchronized (sLock) {
            sBlockingIpcs.push("detectBlockingIpcs");
        }
        try {
            runnable.run();
        } finally {
            synchronized (sLock) {
                sBlockingIpcs.pop();
            }
        }
    } else {
        runnable.run();
    }
}