public static String toString(int type){
    switch(type) {
        case UNKNOWN:
            return "UNKNOWN";
        case GERAN:
            return "GERAN";
        case UTRAN:
            return "UTRAN";
        case EUTRAN:
            return "EUTRAN";
        case CDMA2000:
            return "CDMA2000";
        case IWLAN:
            return "IWLAN";
        case NGRAN:
            return "NGRAN";
        default:
            return Integer.toString(type);
    }
}