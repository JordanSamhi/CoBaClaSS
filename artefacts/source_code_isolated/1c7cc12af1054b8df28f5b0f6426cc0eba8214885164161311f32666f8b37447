public Matrix dotTranspose(Matrix that, Matrix result){
    if (!(this.n == result.n && this.m == that.m && that.n == result.m)) {
        throw new IllegalArgumentException();
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < that.n; j++) {
            double s = 0.0;
            for (int k = 0; k < m; k++) {
                s += this.get(i, k) * that.get(j, k);
            }
            result.put(i, j, s);
        }
    }
    return result;
}