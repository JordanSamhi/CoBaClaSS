public String toString(){
    return new StringBuilder("ApfStats(").append(String.format("%dms ", durationMs)).append(String.format("%dB RA: {", maxProgramSize)).append(String.format("%d received, ", receivedRas)).append(String.format("%d matching, ", matchingRas)).append(String.format("%d dropped, ", droppedRas)).append(String.format("%d zero lifetime, ", zeroLifetimeRas)).append(String.format("%d parse errors}, ", parseErrors)).append(String.format("updates: {all: %d, RAs: %d, allow multicast: %d})", programUpdatesAll, programUpdates, programUpdatesAllowingMulticast)).toString();
}