public void removeConfigCallback(IImsConfigCallback callback) throws ImsException{
    if (DBG)
        Rlog.d(TAG, "removeConfigCallback: " + callback);
    try {
        miConfig.removeImsConfigCallback(callback);
    } catch (RemoteException e) {
        throw new ImsException("removeConfigCallback()", e, ImsReasonInfo.CODE_LOCAL_SERVICE_UNAVAILABLE);
    }
}