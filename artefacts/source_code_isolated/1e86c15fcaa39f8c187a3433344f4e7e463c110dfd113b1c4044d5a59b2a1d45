public String toString(){
    StringBuilder sb = new StringBuilder("[");
    final int size = mX.length;
    for (int i = 0; i < size; i++) {
        if (i != 0) {
            sb.append(", ");
        }
        sb.append("(").append(mX[i]).append(", ").append(mY[i]).append(")");
    }
    sb.append("]");
    return sb.toString();
}