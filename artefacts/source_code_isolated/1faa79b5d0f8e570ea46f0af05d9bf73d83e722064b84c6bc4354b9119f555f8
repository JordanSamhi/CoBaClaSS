public boolean equals(Object obj){
    if (this == obj)
        return true;
    if (obj == null || getClass() != obj.getClass())
        return false;
    final MediaTimestamp that = (MediaTimestamp) obj;
    return (this.mediaTimeUs == that.mediaTimeUs) && (this.nanoTime == that.nanoTime) && (this.clockRate == that.clockRate);
}