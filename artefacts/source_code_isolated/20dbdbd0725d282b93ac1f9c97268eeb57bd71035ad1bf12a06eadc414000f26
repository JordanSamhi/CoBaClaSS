public boolean equals(@Nullable Object o){
    if (this == o)
        return true;
    if (o == null || getClass() != o.getClass())
        return false;
    NotificationStats that = (NotificationStats) o;
    if (mSeen != that.mSeen)
        return false;
    if (mExpanded != that.mExpanded)
        return false;
    if (mDirectReplied != that.mDirectReplied)
        return false;
    if (mSnoozed != that.mSnoozed)
        return false;
    if (mViewedSettings != that.mViewedSettings)
        return false;
    if (mInteracted != that.mInteracted)
        return false;
    return mDismissalSurface == that.mDismissalSurface;
}