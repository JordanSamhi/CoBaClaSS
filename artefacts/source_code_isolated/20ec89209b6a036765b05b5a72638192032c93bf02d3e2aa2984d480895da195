public long getSupportedFeatureSet(@NonNull String ifaceName){
    synchronized (mLock) {
        Iface iface = mIfaceMgr.getIface(ifaceName);
        if (iface == null) {
            Log.e(TAG, "Could not get Iface object for interface " + ifaceName);
            return 0;
        }
        return iface.featureSet;
    }
}