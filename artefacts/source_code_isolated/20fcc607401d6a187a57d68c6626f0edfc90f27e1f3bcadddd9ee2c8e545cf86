public String toString(){
    StringBuilder sb = new StringBuilder();
    sb.append("(");
    for (int i = 0; i < ptypes.length; i++) {
        if (i > 0)
            sb.append(",");
        sb.append(ptypes[i].getSimpleName());
    }
    sb.append(")");
    sb.append(rtype.getSimpleName());
    return sb.toString();
}