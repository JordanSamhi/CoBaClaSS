public void rejectCall(Call ringingCall) throws CallStateException{
    if (VDBG) {
        Rlog.d(LOG_TAG, toString());
    }
    Phone ringingPhone = ringingCall.getPhone();
    ringingPhone.rejectCall();
    if (VDBG) {
        Rlog.d(LOG_TAG, "End rejectCall(" + ringingCall + ")");
        Rlog.d(LOG_TAG, toString());
    }
}