public void preloadImages(Notification notification){
    if (!hasCache()) {
        return;
    }
    retrieveWantedUriSet(notification);
    Set<Uri> wantedSet = getWantedUriSet();
    wantedSet.forEach(uri -> {
        if (!mImageCache.hasEntry(uri)) {
            mImageCache.preload(uri);
        }
    });
}