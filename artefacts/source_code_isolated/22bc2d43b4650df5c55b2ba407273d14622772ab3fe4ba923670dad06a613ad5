public String toStringSimple(){
    return getName() + ": State=" + getCurrentState().getName() + " mApnSetting=" + mApnSetting + " RefCount=" + mApnContexts.size() + " mCid=" + mCid + " mCreateTime=" + mCreateTime + " mLastastFailTime=" + mLastFailTime + " mLastFailCause=" + mLastFailCause + " mTag=" + mTag + " mLinkProperties=" + mLinkProperties + " linkCapabilities=" + getNetworkCapabilities() + " mRestrictedNetworkOverride=" + mRestrictedNetworkOverride;
}