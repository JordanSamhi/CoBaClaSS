public synchronized MtpObject getObject(int id){
    if (id == 0 || id == 0xFFFFFFFF) {
        Log.w(TAG, "Can't get root storages with getObject()");
        return null;
    }
    if (!mObjects.containsKey(id)) {
        Log.w(TAG, "Id " + id + " doesn't exist");
        return null;
    }
    return mObjects.get(id);
}