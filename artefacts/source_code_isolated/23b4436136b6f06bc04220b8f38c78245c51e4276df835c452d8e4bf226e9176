public boolean equals(Object o){
    if (o == null) {
        return false;
    }
    if (!(o instanceof QueueItem)) {
        return false;
    }
    final QueueItem item = (QueueItem) o;
    if (mId != item.mId) {
        return false;
    }
    if (!Objects.equals(mDescription, item.mDescription)) {
        return false;
    }
    return true;
}