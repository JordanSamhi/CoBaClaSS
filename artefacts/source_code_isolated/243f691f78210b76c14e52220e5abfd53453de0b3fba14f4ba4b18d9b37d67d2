public int startSendingOffloadedPacket(@NonNull String ifaceName, int slot, byte[] dstMac, byte[] packet, int protocol, int period){
    byte[] srcMac = NativeUtil.macAddressToByteArray(getMacAddress(ifaceName));
    return mWifiVendorHal.startSendingOffloadedPacket(ifaceName, slot, srcMac, dstMac, packet, protocol, period);
}