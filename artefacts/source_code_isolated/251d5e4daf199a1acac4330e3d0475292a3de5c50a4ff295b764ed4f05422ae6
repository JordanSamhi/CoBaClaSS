public String getDebugString(){
    return "EncodedBuffer( mChunkSize=" + mChunkSize + " mBuffers.size=" + mBuffers.size() + " mBufferCount=" + mBufferCount + " mWriteIndex=" + mWriteIndex + " mWriteBufIndex=" + mWriteBufIndex + " mReadBufIndex=" + mReadBufIndex + " mReadIndex=" + mReadIndex + " mReadableSize=" + mReadableSize + " mReadLimit=" + mReadLimit + " )";
}