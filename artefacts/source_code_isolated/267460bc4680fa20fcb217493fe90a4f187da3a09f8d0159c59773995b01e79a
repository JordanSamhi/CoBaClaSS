public void addUseDifferentAppLabelIfNecessary(ResolverListAdapter adapter){
    if (mCallerChooserTargets != null && mCallerChooserTargets.length > 0) {
        mChooserMultiProfilePagerAdapter.getActiveListAdapter().addServiceResults(null, Lists.newArrayList(mCallerChooserTargets), TARGET_TYPE_DEFAULT, null, mServiceConnections);
    }
}