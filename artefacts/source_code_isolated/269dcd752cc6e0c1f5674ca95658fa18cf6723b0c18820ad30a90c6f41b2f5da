public View.OnClickListener getBubbleClickListener(){
    return new View.OnClickListener() {

        @Override
        public void onClick(View v) {
            Dependency.get(BubbleController.class).onUserChangedBubble(mEntry, !mEntry.isBubble());
            mHeadsUpManager.removeNotification(mEntry.getKey(), true);
        }
    };
}