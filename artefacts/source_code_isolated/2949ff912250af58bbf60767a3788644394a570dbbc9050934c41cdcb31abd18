public boolean equals(Object obj){
    if (this == obj)
        return true;
    if (obj == null)
        return false;
    if (getClass() != obj.getClass())
        return false;
    final ContentCaptureCondition other = (ContentCaptureCondition) obj;
    if (mFlags != other.mFlags)
        return false;
    if (mLocusId == null) {
        if (other.mLocusId != null)
            return false;
    } else {
        if (!mLocusId.equals(other.mLocusId))
            return false;
    }
    return true;
}