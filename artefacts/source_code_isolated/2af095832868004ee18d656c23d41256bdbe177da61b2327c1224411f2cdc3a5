public boolean equals(Object o){
    if (this == o) {
        return true;
    }
    if (!(o instanceof AudioPresentation)) {
        return false;
    }
    AudioPresentation obj = (AudioPresentation) o;
    return mPresentationId == obj.getPresentationId() && mProgramId == obj.getProgramId() && mLanguage.equals(obj.getULocale()) && mMasteringIndication == obj.getMasteringIndication() && mAudioDescriptionAvailable == obj.hasAudioDescription() && mSpokenSubtitlesAvailable == obj.hasSpokenSubtitles() && mDialogueEnhancementAvailable == obj.hasDialogueEnhancement() && mLabels.equals(obj.getULabels());
}