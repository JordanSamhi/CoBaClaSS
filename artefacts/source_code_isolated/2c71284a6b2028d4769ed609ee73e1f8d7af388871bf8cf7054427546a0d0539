public boolean equals(Object obj){
    if (obj == this) {
        return true;
    }
    if (!(obj instanceof RC2ParameterSpec)) {
        return false;
    }
    RC2ParameterSpec other = (RC2ParameterSpec) obj;
    return ((effectiveKeyBits == other.effectiveKeyBits) && java.util.Arrays.equals(iv, other.iv));
}