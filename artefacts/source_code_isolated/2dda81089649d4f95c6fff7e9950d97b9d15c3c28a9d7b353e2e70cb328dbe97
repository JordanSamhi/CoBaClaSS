public boolean removeFirstOccurrence(Object o){
    if (o != null) {
        int mask = elements.length - 1;
        int i = head;
        for (Object x; (x = elements[i]) != null; i = (i + 1) & mask) {
            if (o.equals(x)) {
                delete(i);
                return true;
            }
        }
    }
    return false;
}