public final void onRemoteCapabilityRequest(@NonNull Uri contactUri, @NonNull RcsContactUceCapability remoteInfo, int operationToken) throws ImsException{
    try {
        getListener().onRemoteCapabilityRequest(contactUri, remoteInfo, operationToken);
    } catch (RemoteException e) {
        throw new ImsException(e.getMessage(), ImsException.CODE_ERROR_SERVICE_UNAVAILABLE);
    }
}