public void setMute(boolean muted) throws ImsException{
    synchronized (mLockObj) {
        if (mMute != muted) {
            logi("setMute :: turning mute " + (muted ? "on" : "off"));
            mMute = muted;
            try {
                mSession.setMute(muted);
            } catch (Throwable t) {
                loge("setMute :: ", t);
                throwImsException(t, 0);
            }
        }
    }
}