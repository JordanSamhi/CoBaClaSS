public String toString(){
    return new StringBuilder("SignalThresholdInfo{").append("mSignalMeasurement=").append(mSignalMeasurement).append("mHysteresisMs=").append(mSignalMeasurement).append("mHysteresisDb=").append(mHysteresisDb).append("mThresholds=").append(Arrays.toString(mThresholds)).append("mIsEnabled=").append(mIsEnabled).append("}").toString();
}