public void registrationFailed(int indicationType, android.hardware.radio.V1_5.CellIdentity cellIdentity, String chosenPlmn, @NetworkRegistrationInfo.Domain int domain, int causeCode, int additionalCauseCode){
    mRil.processIndication(indicationType);
    if (cellIdentity == null || TextUtils.isEmpty(chosenPlmn) || (domain & NetworkRegistrationInfo.DOMAIN_CS_PS) == 0 || (domain & ~NetworkRegistrationInfo.DOMAIN_CS_PS) != 0 || causeCode < 0 || additionalCauseCode < 0 || (causeCode == Integer.MAX_VALUE && additionalCauseCode == Integer.MAX_VALUE)) {
        AnomalyReporter.reportAnomaly(UUID.fromString("f16e5703-6105-4341-9eb3-e68189156eb4"), "Invalid registrationFailed indication");
        mRil.riljLoge("Invalid registrationFailed indication");
        return;
    }
    CellIdentity ci = CellIdentity.create(cellIdentity);
    mRil.mRegistrationFailedRegistrant.notifyRegistrant(new AsyncResult(null, new RegistrationFailedEvent(ci, chosenPlmn, domain, causeCode, additionalCauseCode), null));
}