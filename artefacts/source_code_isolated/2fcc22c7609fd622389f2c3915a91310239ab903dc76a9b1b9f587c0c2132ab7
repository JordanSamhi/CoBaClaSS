public String toString(){
    StringBuilder out = new StringBuilder("ScoredNetwork{" + "networkKey=" + networkKey + ", rssiCurve=" + rssiCurve + ", meteredHint=" + meteredHint);
    if (attributes != null && !attributes.isEmpty()) {
        out.append(", attributes=" + attributes);
    }
    out.append('}');
    return out.toString();
}