public void setTtyMode(int ttyMode){
    if (mImsManager == null) {
        Log.w(LOG_TAG, "ImsManager is null when setting TTY mode");
        return;
    }
    try {
        mImsManager.setTtyMode(ttyMode);
    } catch (ImsException e) {
        loge("setTtyMode : " + e);
        retryGetImsService();
    }
}