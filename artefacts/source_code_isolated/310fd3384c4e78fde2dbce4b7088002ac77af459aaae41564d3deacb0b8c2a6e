public int hashCode(){
    int hash = 23;
    hash = hash * 17 + Arrays.hashCode(mSupportedHdrTypes);
    hash = hash * 17 + Float.floatToIntBits(mMaxLuminance);
    hash = hash * 17 + Float.floatToIntBits(mMaxAverageLuminance);
    hash = hash * 17 + Float.floatToIntBits(mMinLuminance);
    return hash;
}