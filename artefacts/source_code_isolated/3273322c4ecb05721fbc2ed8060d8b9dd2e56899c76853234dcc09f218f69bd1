 boolean isSimilar(ChooserTargetInfo other){
    if (other == null)
        return false;
    ChooserTarget ct1 = getChooserTarget();
    ChooserTarget ct2 = other.getChooserTarget();
    if (ct1 == null || ct2 == null)
        return false;
    if (ct1.getComponentName().equals(ct2.getComponentName()) && TextUtils.equals(getDisplayLabel(), other.getDisplayLabel()) && TextUtils.equals(getExtendedInfo(), other.getExtendedInfo())) {
        return true;
    }
    return false;
}