public boolean onBackPressed(boolean hideImmediately){
    if (mBouncer.isShowing()) {
        mStatusBar.endAffordanceLaunch();
        if (mBouncer.isScrimmed() && !mBouncer.needsFullscreenBouncer()) {
            hideBouncer(false);
            updateStates();
        } else {
            reset(hideImmediately);
        }
        return true;
    }
    return false;
}