public boolean setPermittedAccessibilityServices(@NonNull ComponentName admin, List<String> packageNames){
    throwIfParentInstance("setPermittedAccessibilityServices");
    if (mService != null) {
        try {
            return mService.setPermittedAccessibilityServices(admin, packageNames);
        } catch (RemoteException e) {
            throw e.rethrowFromSystemServer();
        }
    }
    return false;
}