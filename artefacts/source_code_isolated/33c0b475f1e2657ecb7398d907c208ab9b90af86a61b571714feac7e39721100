public String toString(){
    return new StringBuilder(TAG).append(":{ mLac=").append(mLac).append(" mCid=").append(mCid).append(" mPsc=").append(mPsc).append(" mUarfcn=").append(mUarfcn).append(" mMcc=").append(mMccStr).append(" mMnc=").append(mMncStr).append(" mAlphaLong=").append(mAlphaLong).append(" mAlphaShort=").append(mAlphaShort).append(" mAdditionalPlmns=").append(mAdditionalPlmns).append(" mCsgInfo=").append(mCsgInfo).append("}").toString();
}