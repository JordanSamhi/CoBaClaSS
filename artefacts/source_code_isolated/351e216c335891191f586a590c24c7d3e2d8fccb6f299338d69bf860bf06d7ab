public boolean equals(Object o){
    if (o == this) {
        return true;
    }
    if (!(o instanceof DisplayViewport)) {
        return false;
    }
    DisplayViewport other = (DisplayViewport) o;
    return valid == other.valid && isActive == other.isActive && displayId == other.displayId && orientation == other.orientation && logicalFrame.equals(other.logicalFrame) && physicalFrame.equals(other.physicalFrame) && deviceWidth == other.deviceWidth && deviceHeight == other.deviceHeight && TextUtils.equals(uniqueId, other.uniqueId) && physicalPort == other.physicalPort && type == other.type;
}