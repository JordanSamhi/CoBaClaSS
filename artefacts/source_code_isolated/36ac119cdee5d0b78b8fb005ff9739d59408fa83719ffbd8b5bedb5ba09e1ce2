public static boolean elementsEqual(@Nullable LongArray a, @Nullable LongArray b){
    if (a == null || b == null)
        return a == b;
    if (a.mSize != b.mSize)
        return false;
    for (int i = 0; i < a.mSize; i++) {
        if (a.get(i) != b.get(i)) {
            return false;
        }
    }
    return true;
}