public static void registerStaticService(@NonNull String serviceName, @NonNull Class<TServiceClass> serviceWrapperClass, @NonNull StaticServiceProducerWithoutBinder<TServiceClass> serviceProducer){
    ensureInitializing("registerStaticService");
    Preconditions.checkStringNotEmpty(serviceName);
    Objects.requireNonNull(serviceWrapperClass);
    Objects.requireNonNull(serviceProducer);
    registerService(serviceName, serviceWrapperClass, new StaticServiceFetcher<TServiceClass>() {

        @Override
        public TServiceClass createService() {
            return serviceProducer.createService();
        }
    });
}