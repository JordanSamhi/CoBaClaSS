public VolumeInfo findPrivateForEmulated(VolumeInfo emulatedVol){
    if (emulatedVol != null) {
        String id = emulatedVol.getId();
        int idx = id.indexOf(";");
        if (idx != -1) {
            id = id.substring(0, idx);
        }
        return findVolumeById(id.replace("emulated", "private"));
    } else {
        return null;
    }
}