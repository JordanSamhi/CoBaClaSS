public static ICharSequenceResultCallback.Stub of(@NonNull CancellationGroup.Completable.CharSequence value){
    final AtomicReference<WeakReference<CancellationGroup.Completable.CharSequence>> atomicRef = new AtomicReference<>(new WeakReference<>(value));
    return new ICharSequenceResultCallback.Stub() {

        @BinderThread
        @Override
        public void onResult(CharSequence result) {
            final CancellationGroup.Completable.CharSequence value = unwrap(atomicRef);
            if (value == null) {
                return;
            }
            value.onComplete(result);
        }
    };
}