public boolean equals(Object o){
    if (this == o)
        return true;
    if (!(o instanceof SignalThresholdInfo)) {
        return false;
    }
    SignalThresholdInfo other = (SignalThresholdInfo) o;
    return mSignalMeasurement == other.mSignalMeasurement && mHysteresisMs == other.mHysteresisMs && mHysteresisDb == other.mHysteresisDb && Arrays.equals(mThresholds, other.mThresholds) && mIsEnabled == other.mIsEnabled;
}