public String toString(){
    return new StringBuilder(TAG + ":{").append(" mPci = ").append(mPci).append(" mTac = ").append(mTac).append(" mNrArfcn = ").append(mNrArfcn).append(" mBands = ").append(Arrays.toString(mBands)).append(" mMcc = ").append(mMccStr).append(" mMnc = ").append(mMncStr).append(" mNci = ").append(mNci).append(" mAlphaLong = ").append(mAlphaLong).append(" mAlphaShort = ").append(mAlphaShort).append(" mAdditionalPlmns = ").append(mAdditionalPlmns).append(" }").toString();
}