public static String getLoggingLevelAsString(@LoggingLevel int level){
    switch(level) {
        case LOGGING_LEVEL_OFF:
            return "OFF";
        case LOGGING_LEVEL_DEBUG:
            return "DEBUG";
        case LOGGING_LEVEL_VERBOSE:
            return "VERBOSE";
        default:
            return "UNKNOWN-" + level;
    }
}