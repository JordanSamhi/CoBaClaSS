public void reportSuccessfulBiometricUnlock(boolean isStrongBiometric, int userId){
    if (DEBUG) {
        Slog.d(TAG, "reportSuccessfulBiometricUnlock for isStrongBiometric=" + isStrongBiometric + ", userId=" + userId);
    }
    final int argNotUsed = 0;
    if (isStrongBiometric) {
        mHandler.obtainMessage(MSG_STRONG_BIOMETRIC_UNLOCK, userId, argNotUsed).sendToTarget();
    } else {
        mHandler.obtainMessage(MSG_SCHEDULE_NON_STRONG_BIOMETRIC_TIMEOUT, userId, argNotUsed).sendToTarget();
    }
}