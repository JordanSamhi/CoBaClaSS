public void removeOnMediaKeyEventSessionChangedListener(@NonNull OnMediaKeyEventSessionChangedListener listener){
    if (listener == null) {
        throw new NullPointerException("listener shouldn't be null");
    }
    synchronized (mLock) {
        try {
            mMediaKeyEventSessionChangedCallbacks.remove(listener);
            if (mMediaKeyEventSessionChangedCallbacks.size() == 0) {
                mService.removeOnMediaKeyEventSessionChangedListener(mOnMediaKeyEventSessionChangedListenerStub);
            }
        } catch (RemoteException e) {
            Log.e(TAG, "Failed to set media key listener", e);
        }
    }
}