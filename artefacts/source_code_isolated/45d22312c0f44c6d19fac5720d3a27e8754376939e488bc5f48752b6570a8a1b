public void setUp(Context context, boolean needsLinear){
    final String[] coefficients = context.getResources().getStringArray(needsLinear ? R.array.config_nightDisplayColorTemperatureCoefficients : R.array.config_nightDisplayColorTemperatureCoefficientsNative);
    for (int i = 0; i < 9 && i < coefficients.length; i++) {
        mColorTempCoefficients[i] = Float.parseFloat(coefficients[i]);
    }
}