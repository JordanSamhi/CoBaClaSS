public String toString(){
    StringBuffer sb = new StringBuffer();
    sb.append("[");
    for (int i = 0; i < mCount; i++) {
        final int index = offsetOf(i);
        sb.append(mValues[index] + " @ " + mTimes[index]);
        if (i + 1 != mCount) {
            sb.append(", ");
        }
    }
    sb.append("]");
    return sb.toString();
}