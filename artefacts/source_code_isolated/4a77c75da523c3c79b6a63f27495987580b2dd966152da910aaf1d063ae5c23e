public ImsServiceInfo getImsServiceInfoFromCache(String packageName){
    if (TextUtils.isEmpty(packageName)) {
        return null;
    }
    ImsServiceInfo infoFilter = getInfoByPackageName(mInstalledServicesCache, packageName);
    if (infoFilter != null) {
        return infoFilter;
    } else {
        return null;
    }
}