public void queryFdn(){
    int serviceClassX;
    serviceClassX = CommandsInterface.SERVICE_CLASS_VOICE + CommandsInterface.SERVICE_CLASS_DATA + CommandsInterface.SERVICE_CLASS_FAX;
    mCi.queryFacilityLockForApp(CommandsInterface.CB_FACILITY_BA_FD, "", serviceClassX, mAid, mHandler.obtainMessage(EVENT_QUERY_FACILITY_FDN_DONE));
}