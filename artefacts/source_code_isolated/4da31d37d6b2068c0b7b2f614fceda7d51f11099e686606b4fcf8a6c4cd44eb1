public boolean equals(Object o){
    if (this == o)
        return true;
    if (o == null || getClass() != o.getClass())
        return false;
    final PasswordMetrics that = (PasswordMetrics) o;
    return credType == that.credType && length == that.length && letters == that.letters && upperCase == that.upperCase && lowerCase == that.lowerCase && numeric == that.numeric && symbols == that.symbols && nonLetter == that.nonLetter && nonNumeric == that.nonNumeric && seqLength == that.seqLength;
}