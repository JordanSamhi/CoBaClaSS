public boolean equals(@Nullable Object obj){
    if (obj instanceof SubscriptionPlan) {
        final SubscriptionPlan other = (SubscriptionPlan) obj;
        return Objects.equals(cycleRule, other.cycleRule) && Objects.equals(title, other.title) && Objects.equals(summary, other.summary) && dataLimitBytes == other.dataLimitBytes && dataLimitBehavior == other.dataLimitBehavior && dataUsageBytes == other.dataUsageBytes && dataUsageTime == other.dataUsageTime && Arrays.equals(networkTypes, other.networkTypes);
    }
    return false;
}