public int startOp(int op, int uid, @Nullable String packageName, boolean startIfModeDefault, @Nullable String attributionTag, @Nullable String message){
    final int mode = startOpNoThrow(op, uid, packageName, startIfModeDefault, attributionTag, message);
    if (mode == MODE_ERRORED) {
        throw new SecurityException(buildSecurityExceptionMsg(op, uid, packageName));
    }
    return mode;
}