public PasspointProvider makePasspointProvider(PasspointConfiguration config, WifiKeyStore keyStore, WifiCarrierInfoManager wifiCarrierInfoManager, long providerId, int creatorUid, String packageName, boolean isFromSuggestion){
    return new PasspointProvider(config, keyStore, wifiCarrierInfoManager, providerId, creatorUid, packageName, isFromSuggestion);
}