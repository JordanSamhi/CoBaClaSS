public static boolean logBinaryPushStateChanged(@NonNull String trainName, long trainVersionCode, int options, int state, @NonNull long[] experimentIds){
    ProtoOutputStream proto = new ProtoOutputStream();
    for (long id : experimentIds) {
        proto.write(ProtoOutputStream.FIELD_TYPE_INT64 | ProtoOutputStream.FIELD_COUNT_REPEATED | EXPERIMENT_IDS_FIELD_ID, id);
    }
    StatsdStatsLog.write(StatsdStatsLog.BINARY_PUSH_STATE_CHANGED, trainName, trainVersionCode, (options & IStatsd.FLAG_REQUIRE_STAGING) > 0, (options & IStatsd.FLAG_ROLLBACK_ENABLED) > 0, (options & IStatsd.FLAG_REQUIRE_LOW_LATENCY_MONITOR) > 0, state, proto.getBytes(), 0, 0, false);
    return true;
}