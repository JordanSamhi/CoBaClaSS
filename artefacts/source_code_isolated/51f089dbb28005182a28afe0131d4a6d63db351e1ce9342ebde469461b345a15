public void migrateTun(int tunUid, @NonNull String tunIface, @NonNull String[] underlyingIfaces){
    final Entry tunIfaceTotal = new Entry();
    final Entry[] perInterfaceTotal = new Entry[underlyingIfaces.length];
    final Entry underlyingIfacesTotal = new Entry();
    for (int i = 0; i < perInterfaceTotal.length; i++) {
        perInterfaceTotal[i] = new Entry();
    }
    tunAdjustmentInit(tunUid, tunIface, underlyingIfaces, tunIfaceTotal, perInterfaceTotal, underlyingIfacesTotal);
    final Entry[] moved = addTrafficToApplications(tunUid, tunIface, underlyingIfaces, tunIfaceTotal, perInterfaceTotal, underlyingIfacesTotal);
    deductTrafficFromVpnApp(tunUid, underlyingIfaces, moved);
}