public void commitRollback(int rollbackId, @NonNull List<VersionedPackage> causePackages, @NonNull IntentSender statusReceiver){
    try {
        mBinder.commitRollback(rollbackId, new ParceledListSlice(causePackages), mCallerPackageName, statusReceiver);
    } catch (RemoteException e) {
        throw e.rethrowFromSystemServer();
    }
}