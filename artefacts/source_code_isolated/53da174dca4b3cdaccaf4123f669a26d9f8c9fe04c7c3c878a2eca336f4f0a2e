public static Collection<Object[]> getParameters(){
    return Arrays.asList(new Object[][] { { "Visible", new int[] { View.VISIBLE } }, { "Invisible~Visible", new int[] { View.INVISIBLE, View.VISIBLE } }, { "Gone~Visible", new int[] { View.GONE, View.VISIBLE } }, { "Gone~Invisible", new int[] { View.GONE, View.INVISIBLE } } });
}