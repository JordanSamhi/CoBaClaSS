public String toString(){
    return "SmsCbHeader{GS=" + mGeographicalScope + ", serialNumber=0x" + Integer.toHexString(mSerialNumber) + ", messageIdentifier=0x" + Integer.toHexString(mMessageIdentifier) + ", format=" + mFormat + ", DCS=0x" + Integer.toHexString(mDataCodingScheme) + ", page " + mPageIndex + " of " + mNrOfPages + '}';
}