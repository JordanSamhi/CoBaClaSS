public boolean equals(Object o){
    if (this == o) {
        return true;
    }
    if (!(o instanceof RouteDiscoveryPreference)) {
        return false;
    }
    RouteDiscoveryPreference other = (RouteDiscoveryPreference) o;
    return Objects.equals(mPreferredFeatures, other.mPreferredFeatures) && mShouldPerformActiveScan == other.mShouldPerformActiveScan;
}