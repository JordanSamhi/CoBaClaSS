public boolean equals(Object o){
    if (o instanceof BluetoothCodecStatus) {
        BluetoothCodecStatus other = (BluetoothCodecStatus) o;
        return (Objects.equals(other.mCodecConfig, mCodecConfig) && sameCapabilities(other.mCodecsLocalCapabilities, mCodecsLocalCapabilities) && sameCapabilities(other.mCodecsSelectableCapabilities, mCodecsSelectableCapabilities));
    }
    return false;
}