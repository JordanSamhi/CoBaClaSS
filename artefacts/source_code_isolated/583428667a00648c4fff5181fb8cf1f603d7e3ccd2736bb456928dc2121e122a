public void registerPredictionUpdatesLocked(@NonNull AppPredictionSessionId sessionId, @NonNull IPredictionCallback callback){
    final boolean serviceExists = resolveService(sessionId, s -> s.registerPredictionUpdates(sessionId, callback), false);
    final AppPredictionSessionInfo sessionInfo = mSessionInfos.get(sessionId);
    if (serviceExists && sessionInfo != null) {
        sessionInfo.addCallbackLocked(callback);
    }
}