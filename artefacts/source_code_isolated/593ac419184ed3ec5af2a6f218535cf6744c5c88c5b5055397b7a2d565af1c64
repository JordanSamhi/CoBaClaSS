public synchronized int indexOf(Object o, int index){
    if (o == null) {
        for (int i = index; i < elementCount; i++) if (elementData[i] == null)
            return i;
    } else {
        for (int i = index; i < elementCount; i++) if (o.equals(elementData[i]))
            return i;
    }
    return -1;
}