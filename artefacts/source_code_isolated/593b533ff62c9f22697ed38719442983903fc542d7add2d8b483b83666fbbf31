public int presentConfirmationPrompt(IBinder listener, String promptText, byte[] extraData, String locale, int uiOptionsAsFlags){
    try {
        return mBinder.presentConfirmationPrompt(listener, promptText, extraData, locale, uiOptionsAsFlags);
    } catch (RemoteException e) {
        Log.w(TAG, "Cannot connect to keystore", e);
        return CONFIRMATIONUI_SYSTEM_ERROR;
    }
}