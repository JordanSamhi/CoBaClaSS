public boolean equals(Object o){
    if (this == o)
        return true;
    if (o == null || getClass() != o.getClass())
        return false;
    NotifyingApp that = (NotifyingApp) o;
    return getUserId() == that.getUserId() && getLastNotified() == that.getLastNotified() && Objects.equals(mPkg, that.mPkg);
}