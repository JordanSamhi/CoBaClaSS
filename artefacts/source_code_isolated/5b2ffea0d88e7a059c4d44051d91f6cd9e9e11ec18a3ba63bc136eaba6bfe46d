public Builder addDataset(@Nullable Dataset dataset){
    throwIfDestroyed();
    throwIfDisableAutofillCalled();
    if (dataset == null) {
        return this;
    }
    if (mDatasets == null) {
        mDatasets = new ArrayList<>();
    }
    if (!mDatasets.add(dataset)) {
        return this;
    }
    return this;
}