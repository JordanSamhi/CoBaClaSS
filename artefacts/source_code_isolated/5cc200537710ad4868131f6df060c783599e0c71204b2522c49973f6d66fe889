public CharSequence getHeadsUpStatusBarText(){
    CharSequence conversationTitle = !TextUtils.isEmpty(super.mBigContentTitle) ? super.mBigContentTitle : mConversationTitle;
    if (mConversationType == CONVERSATION_TYPE_LEGACY && !TextUtils.isEmpty(conversationTitle) && !hasOnlyWhiteSpaceSenders()) {
        return conversationTitle;
    }
    return null;
}