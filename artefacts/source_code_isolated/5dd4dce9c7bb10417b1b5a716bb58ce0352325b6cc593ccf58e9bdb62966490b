public boolean sendDtmf(char c){
    boolean result = false;
    if (VDBG) {
        Rlog.d(LOG_TAG, " sendDtmf(" + c + ")");
        Rlog.d(LOG_TAG, toString());
    }
    if (hasActiveFgCall()) {
        getActiveFgCall().getPhone().sendDtmf(c);
        result = true;
    }
    if (VDBG) {
        Rlog.d(LOG_TAG, "End sendDtmf(" + c + ")");
        Rlog.d(LOG_TAG, toString());
    }
    return result;
}