public static boolean channelEquals(@Nullable WifiNative.ChannelSettings channel1, @Nullable WifiNative.ChannelSettings channel2){
    if (channel1 == null || channel2 == null)
        return false;
    if (channel1 == channel2)
        return true;
    if (channel1.frequency != channel2.frequency)
        return false;
    if (channel1.dwell_time_ms != channel2.dwell_time_ms)
        return false;
    return channel1.passive == channel2.passive;
}