public Builder setPhysicalCameraKey(@NonNull Key<T> key, T value, @NonNull String physicalCameraId){
    if (!mRequest.mPhysicalCameraSettings.containsKey(physicalCameraId)) {
        throw new IllegalArgumentException("Physical camera id: " + physicalCameraId + " is not valid!");
    }
    mRequest.mPhysicalCameraSettings.get(physicalCameraId).set(key, value);
    return this;
}