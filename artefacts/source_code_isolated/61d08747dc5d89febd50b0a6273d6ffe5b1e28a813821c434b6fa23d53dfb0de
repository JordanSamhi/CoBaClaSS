public int getNotGoneChildCount(){
    int childCount = getChildCount();
    int count = 0;
    for (int i = 0; i < childCount; i++) {
        ExpandableView child = (ExpandableView) getChildAt(i);
        if (child.getVisibility() != View.GONE && !child.willBeGone() && child != mShelf) {
            count++;
        }
    }
    return count;
}