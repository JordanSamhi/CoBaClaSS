public boolean supportsConversionOfCdmaCallerIdMmiCodesWhileRoaming(){
    CarrierConfigManager configManager = (CarrierConfigManager) getContext().getSystemService(Context.CARRIER_CONFIG_SERVICE);
    PersistableBundle b = configManager.getConfigForSubId(getSubId());
    if (b != null) {
        return b.getBoolean(CarrierConfigManager.KEY_CONVERT_CDMA_CALLER_ID_MMI_CODES_WHILE_ROAMING_ON_3GPP_BOOL, false);
    } else {
        return false;
    }
}