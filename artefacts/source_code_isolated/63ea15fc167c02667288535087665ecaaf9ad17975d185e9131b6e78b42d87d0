public void setEchoSuppressionEnabled(){
    if (VDBG) {
        Rlog.d(LOG_TAG, " setEchoSuppression()");
        Rlog.d(LOG_TAG, toString());
    }
    if (hasActiveFgCall()) {
        getActiveFgCall().getPhone().setEchoSuppressionEnabled();
    }
    if (VDBG) {
        Rlog.d(LOG_TAG, "End setEchoSuppression()");
        Rlog.d(LOG_TAG, toString());
    }
}