public boolean equals(@Nullable Object obj){
    if (this == obj) {
        return true;
    }
    if (obj == null || getClass() != obj.getClass()) {
        return false;
    }
    UiccAccessRule that = (UiccAccessRule) obj;
    return Arrays.equals(mCertificateHash, that.mCertificateHash) && Objects.equals(mPackageName, that.mPackageName) && mAccessType == that.mAccessType;
}