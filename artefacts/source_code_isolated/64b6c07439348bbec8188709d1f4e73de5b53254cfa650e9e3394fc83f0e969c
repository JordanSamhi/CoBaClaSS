public static boolean interfacesEqual(IHwInterface lft, Object rgt){
    if (lft == rgt) {
        return true;
    }
    if (lft == null || rgt == null) {
        return false;
    }
    if (!(rgt instanceof IHwInterface)) {
        return false;
    }
    return Objects.equals(lft.asBinder(), ((IHwInterface) rgt).asBinder());
}