public boolean isValid(@NonNull ValueFinder finder){
    final String value = finder.findByAutofillId(mId);
    if (value == null) {
        Log.w(TAG, "No view for id " + mId);
        return false;
    }
    final boolean valid = mRegex.matcher(value).matches();
    if (sDebug)
        Log.d(TAG, "isValid(): " + valid);
    return valid;
}