public String toString(){
    return new StringBuilder().append("SignalStrength:{").append("mCdma=").append(mCdma).append(",mGsm=").append(mGsm).append(",mWcdma=").append(mWcdma).append(",mTdscdma=").append(mTdscdma).append(",mLte=").append(mLte).append(",mNr=").append(mNr).append(",primary=").append(getPrimary().getClass().getSimpleName()).append("}").toString();
}