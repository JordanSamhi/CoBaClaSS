public boolean equals(Object o){
    if (this == o) {
        return true;
    }
    if (o == null || getClass() != o.getClass()) {
        return false;
    }
    final MoveToDisplayItem other = (MoveToDisplayItem) o;
    return mTargetDisplayId == other.mTargetDisplayId && Objects.equals(mConfiguration, other.mConfiguration);
}