public static boolean hasEmsSupport(){
    if (!isNoEmsSupportConfigListExisted()) {
        return true;
    }
    String simOperator;
    String gid;
    final long identity = Binder.clearCallingIdentity();
    try {
        simOperator = TelephonyManager.getDefault().getSimOperatorNumeric();
        gid = TelephonyManager.getDefault().getGroupIdLevel1();
    } finally {
        Binder.restoreCallingIdentity(identity);
    }
    if (!TextUtils.isEmpty(simOperator)) {
        for (NoEmsSupportConfig currentConfig : mNoEmsSupportConfigList) {
            if (simOperator.startsWith(currentConfig.mOperatorNumber) && (TextUtils.isEmpty(currentConfig.mGid1) || (!TextUtils.isEmpty(currentConfig.mGid1) && currentConfig.mGid1.equalsIgnoreCase(gid)))) {
                return false;
            }
        }
    }
    return true;
}