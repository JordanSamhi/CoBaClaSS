public boolean equals(Object o){
    if (this == o) {
        return true;
    }
    if (o == null || getClass() != o.getClass()) {
        return false;
    }
    final ResumeActivityItem other = (ResumeActivityItem) o;
    return mProcState == other.mProcState && mUpdateProcState == other.mUpdateProcState && mIsForward == other.mIsForward;
}