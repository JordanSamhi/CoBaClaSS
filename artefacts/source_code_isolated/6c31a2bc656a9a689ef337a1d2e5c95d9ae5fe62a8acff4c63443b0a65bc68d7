public boolean startsWith(String prefix, int toffset){
    int to = toffset;
    int po = 0;
    int pc = prefix.length();
    if ((toffset < 0) || (toffset > length() - pc)) {
        return false;
    }
    while (--pc >= 0) {
        if (charAt(to++) != prefix.charAt(po++)) {
            return false;
        }
    }
    return true;
}