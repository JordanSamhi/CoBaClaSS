public static boolean shouldLogSlowQuery(long elapsedTimeMillis){
    final int slowQueryMillis = Math.min(SystemProperties.getInt(NoPreloadHolder.SLOW_QUERY_THRESHOLD_PROP, Integer.MAX_VALUE), SystemProperties.getInt(NoPreloadHolder.SLOW_QUERY_THRESHOLD_UID_PROP, Integer.MAX_VALUE));
    return elapsedTimeMillis >= slowQueryMillis;
}