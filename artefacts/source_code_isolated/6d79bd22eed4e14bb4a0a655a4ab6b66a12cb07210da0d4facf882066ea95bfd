public String toString(){
    return new StringBuilder(TAG).append(":{ mMcc=").append(mMccStr).append(" mMnc=").append(mMncStr).append(" mAlphaLong=").append(mAlphaLong).append(" mAlphaShort=").append(mAlphaShort).append(" mLac=").append(mLac).append(" mCid=").append(mCid).append(" mCpid=").append(mCpid).append(" mUarfcn=").append(mUarfcn).append(" mAdditionalPlmns=").append(mAdditionalPlmns).append(" mCsgInfo=").append(mCsgInfo).append("}").toString();
}