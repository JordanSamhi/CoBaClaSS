public boolean equals(@Nullable Object obj){
    if (!(obj instanceof RemoteUserInfo)) {
        return false;
    }
    if (this == obj) {
        return true;
    }
    RemoteUserInfo otherUserInfo = (RemoteUserInfo) obj;
    return TextUtils.equals(mPackageName, otherUserInfo.mPackageName) && mPid == otherUserInfo.mPid && mUid == otherUserInfo.mUid;
}