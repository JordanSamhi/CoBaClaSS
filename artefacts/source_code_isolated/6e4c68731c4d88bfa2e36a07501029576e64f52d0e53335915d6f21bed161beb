public boolean equals(@Nullable Object object){
    if (object == this) {
        return true;
    }
    boolean isEqual = false;
    if (object instanceof MemoryRegion) {
        MemoryRegion other = (MemoryRegion) object;
        isEqual = (other.getCapacityBytes() == mSizeBytes) && (other.getFreeCapacityBytes() == mSizeBytesFree) && (other.isReadable() == mIsReadable) && (other.isWritable() == mIsWritable) && (other.isExecutable() == mIsExecutable);
    }
    return isEqual;
}