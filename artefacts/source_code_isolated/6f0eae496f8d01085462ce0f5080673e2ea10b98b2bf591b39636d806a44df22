public boolean equals(Object o){
    if (this == o) {
        return true;
    }
    if (o == null || getClass() != o.getClass()) {
        return false;
    }
    final PauseActivityItem other = (PauseActivityItem) o;
    return mFinished == other.mFinished && mUserLeaving == other.mUserLeaving && mConfigChanges == other.mConfigChanges && mDontReport == other.mDontReport;
}