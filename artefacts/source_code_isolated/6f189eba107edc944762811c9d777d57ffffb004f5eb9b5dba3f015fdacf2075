public void removeOnOpportunisticSubscriptionsChangedListener(@NonNull SubscriptionManager.OnOpportunisticSubscriptionsChangedListener listener){
    try {
        sRegistry.removeOnSubscriptionsChangedListener(mContext.getOpPackageName(), mOpportunisticSubscriptionChangedListenerMap.get(listener));
        mOpportunisticSubscriptionChangedListenerMap.remove(listener);
    } catch (RemoteException ex) {
    }
}