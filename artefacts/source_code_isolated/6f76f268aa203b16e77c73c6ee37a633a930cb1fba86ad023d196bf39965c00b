public void simSlotsStatusChanged(int indicationType, ArrayList<android.hardware.radio.config.V1_0.SimSlotStatus> slotStatus){
    ArrayList<IccSlotStatus> ret = RadioConfig.convertHalSlotStatus(slotStatus);
    Rlog.d(TAG, "[UNSL]< " + " UNSOL_SIM_SLOT_STATUS_CHANGED " + ret.toString());
    if (mRadioConfig.mSimSlotStatusRegistrant != null) {
        mRadioConfig.mSimSlotStatusRegistrant.notifyRegistrant(new AsyncResult(null, ret, null));
    }
}