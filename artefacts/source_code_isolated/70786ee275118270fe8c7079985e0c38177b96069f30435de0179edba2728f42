public boolean equals(@Nullable Object obj){
    if (obj == null || !(obj.getClass().equals(ApfProgramEvent.class)))
        return false;
    final ApfProgramEvent other = (ApfProgramEvent) obj;
    return lifetime == other.lifetime && actualLifetime == other.actualLifetime && filteredRas == other.filteredRas && currentRas == other.currentRas && programLength == other.programLength && flags == other.flags;
}