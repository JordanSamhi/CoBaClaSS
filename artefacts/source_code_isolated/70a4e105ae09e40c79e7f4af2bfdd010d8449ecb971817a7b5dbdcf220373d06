public void testGetConstant_whenSettingHasInvalidType_doesNotUpdateParamsOrThrow(){
    boolean testValue = true;
    final String invalidSettingType = BackupManagerConstants.KEY_VALUE_BACKUP_REQUIRED_NETWORK_TYPE + "=" + testValue;
    putStringAndNotify(invalidSettingType);
    int keyValueBackupRequiredNetworkType = mConstants.getKeyValueBackupRequiredNetworkType();
    assertThat(keyValueBackupRequiredNetworkType).isEqualTo(BackupManagerConstants.DEFAULT_KEY_VALUE_BACKUP_REQUIRED_NETWORK_TYPE);
}