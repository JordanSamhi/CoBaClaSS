public void getDownloadableSubscriptionMetadata(DownloadableSubscription subscription, PendingIntent callbackIntent){
    if (!isEnabled()) {
        sendUnavailableError(callbackIntent);
        return;
    }
    try {
        getIEuiccController().getDownloadableSubscriptionMetadata(mCardId, subscription, mContext.getOpPackageName(), callbackIntent);
    } catch (RemoteException e) {
        throw e.rethrowFromSystemServer();
    }
}