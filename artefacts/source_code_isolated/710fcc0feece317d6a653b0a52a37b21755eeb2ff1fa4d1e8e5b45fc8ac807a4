public long getTimeInRxSignalStrengthLevelMicros(@IntRange(from = CellSignalStrength.SIGNAL_STRENGTH_NONE_OR_UNKNOWN, to = CellSignalStrength.SIGNAL_STRENGTH_GREAT) int signalStrengthBin){
    if (signalStrengthBin >= mTimeInRxSignalStrengthLevelMs.length) {
        return -1;
    }
    return mTimeInRxSignalStrengthLevelMs[signalStrengthBin];
}