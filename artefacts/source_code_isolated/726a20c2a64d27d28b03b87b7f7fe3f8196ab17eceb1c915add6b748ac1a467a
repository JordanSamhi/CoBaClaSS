public CharSequence getDataManagementLabel(String transportName){
    mContext.enforceCallingOrSelfPermission(android.Manifest.permission.BACKUP, "getDataManagementLabel");
    try {
        CharSequence label = mTransportManager.getTransportDataManagementLabel(transportName);
        if (MORE_DEBUG) {
            Slog.d(TAG, addUserIdToLogMessage(mUserId, "getDataManagementLabel() returning " + label));
        }
        return label;
    } catch (TransportNotRegisteredException e) {
        Slog.e(TAG, addUserIdToLogMessage(mUserId, "Unable to get management label from transport: " + e.getMessage()));
        return null;
    }
}