public boolean equals(Object obj){
    if (!(obj instanceof LowpanProvision)) {
        return false;
    }
    LowpanProvision rhs = (LowpanProvision) obj;
    if (!mIdentity.equals(rhs.mIdentity)) {
        return false;
    }
    if (!Objects.equals(mCredential, rhs.mCredential)) {
        return false;
    }
    return true;
}