public final void drawText(@NonNull char[] text, int index, int count, float x, float y, @NonNull Paint paint){
    if ((index | count | (index + count) | (text.length - index - count)) < 0) {
        throw new IndexOutOfBoundsException();
    }
    nDrawText(mNativeCanvasWrapper, text, index, count, x, y, paint.mBidiFlags, paint.getNativeInstance());
}