public String toString(){
    return new StringBuilder().append("ParcelableConnection [act:").append(mPhoneAccount).append("], state:").append(mState).append(", capabilities:").append(Connection.capabilitiesToString(mConnectionCapabilities)).append(", properties:").append(Connection.propertiesToString(mConnectionProperties)).append(", extras:").append(mExtras).append(", parent:").append(mParentCallId).append(", callDirection:").append(mCallDirection).toString();
}