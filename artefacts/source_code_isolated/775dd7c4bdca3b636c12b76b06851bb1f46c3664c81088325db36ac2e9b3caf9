public Uri insert(@NonNull Uri url, @Nullable ContentValues initialValues, @Nullable Bundle extras) throws RemoteException{
    Objects.requireNonNull(url, "url");
    beforeRemote();
    try {
        return mContentProvider.insert(mPackageName, mAttributionTag, url, initialValues, extras);
    } catch (DeadObjectException e) {
        if (!mStable) {
            mContentResolver.unstableProviderDied(mContentProvider);
        }
        throw e;
    } finally {
        afterRemote();
    }
}