public boolean equals(Object o){
    if (this == o)
        return true;
    if (o == null || !(o instanceof Descriptor))
        return false;
    Descriptor that = (Descriptor) o;
    return (type.equals(that.type) && uuid.equals(that.uuid) && connectMode.equals(that.connectMode) && name.equals(that.name) && implementor.equals(that.implementor));
}