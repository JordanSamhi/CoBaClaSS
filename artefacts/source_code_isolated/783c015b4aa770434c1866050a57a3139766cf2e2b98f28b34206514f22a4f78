public String[] getIsimImpu(int subId){
    return callPhoneMethodForSubIdWithPrivilegedCheck(subId, "getIsimImpu", (phone) -> {
        IsimRecords isim = phone.getIsimRecords();
        if (isim != null) {
            return isim.getIsimImpu();
        } else {
            return null;
        }
    });
}