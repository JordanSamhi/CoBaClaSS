public Map<WifiNetworkSuggestion, List<ScanResult>> getMatchingScanResults(@NonNull List<WifiNetworkSuggestion> wifiNetworkSuggestions, @NonNull List<ScanResult> scanResults){
    Map<WifiNetworkSuggestion, List<ScanResult>> filteredScanResults = new HashMap<>();
    if (wifiNetworkSuggestions == null || wifiNetworkSuggestions.isEmpty() || scanResults == null || scanResults.isEmpty()) {
        return filteredScanResults;
    }
    for (WifiNetworkSuggestion suggestion : wifiNetworkSuggestions) {
        if (suggestion == null || suggestion.wifiConfiguration == null) {
            continue;
        }
        filteredScanResults.put(suggestion, getMatchingScanResultsForSuggestion(suggestion, scanResults));
    }
    return filteredScanResults;
}