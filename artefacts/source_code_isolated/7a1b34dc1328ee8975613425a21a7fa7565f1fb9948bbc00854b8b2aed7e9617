public final StatusBarNotification[] getSnoozedNotifications(){
    try {
        ParceledListSlice<StatusBarNotification> parceledList = getNotificationInterface().getSnoozedNotificationsFromListener(mWrapper, TRIM_FULL);
        return cleanUpNotificationList(parceledList);
    } catch (android.os.RemoteException ex) {
        Log.v(TAG, "Unable to contact notification manager", ex);
    }
    return null;
}