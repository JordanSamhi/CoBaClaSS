public Builder setValue(@NonNull AutofillId id, @Nullable AutofillValue value, @Nullable Pattern filter, @NonNull RemoteViews presentation){
    throwIfDestroyed();
    Preconditions.checkNotNull(presentation, "presentation cannot be null");
    setLifeTheUniverseAndEverything(id, value, presentation, null, new DatasetFieldFilter(filter));
    return this;
}