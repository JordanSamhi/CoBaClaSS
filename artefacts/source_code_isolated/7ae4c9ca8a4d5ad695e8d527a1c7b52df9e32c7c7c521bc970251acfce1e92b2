public void testUpdateTransportAttributes_whenTransportUidNotEqualToCallingUid_throwsException() throws Exception{
    setUpForUpdateTransportAttributes();
    mShadowContext.grantPermissions(android.Manifest.permission.BACKUP);
    UserBackupManagerService backupManagerService = createUserBackupManagerServiceAndRunTasks();
    expectThrows(SecurityException.class, () -> backupManagerService.updateTransportAttributes(mTransportUid + 1, mTransportComponent, mTransportName, new Intent(), "currentDestinationString", new Intent(), "dataManagementLabel"));
}