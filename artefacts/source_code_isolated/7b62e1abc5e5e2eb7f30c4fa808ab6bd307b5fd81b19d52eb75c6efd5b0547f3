public boolean equals(Object obj){
    if (!(obj instanceof LowpanChannelInfo)) {
        return false;
    }
    LowpanChannelInfo rhs = (LowpanChannelInfo) obj;
    return Objects.equals(mName, rhs.mName) && mIndex == rhs.mIndex && mIsMaskedByRegulatoryDomain == rhs.mIsMaskedByRegulatoryDomain && mSpectrumCenterFrequency == rhs.mSpectrumCenterFrequency && mSpectrumBandwidth == rhs.mSpectrumBandwidth && mMaxTransmitPower == rhs.mMaxTransmitPower;
}