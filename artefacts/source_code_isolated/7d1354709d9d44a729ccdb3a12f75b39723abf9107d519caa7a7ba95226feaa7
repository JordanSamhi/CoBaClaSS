public static boolean sameCapabilities(BluetoothCodecConfig[] c1, BluetoothCodecConfig[] c2){
    if (c1 == null) {
        return (c2 == null);
    }
    if (c2 == null) {
        return false;
    }
    if (c1.length != c2.length) {
        return false;
    }
    return Arrays.asList(c1).containsAll(Arrays.asList(c2));
}