public void requestLatestTetheringEntitlementResult(@TetheringType final int type, @NonNull final ResultReceiver receiver, final boolean showEntitlementUi){
    final String callerPkg = mContext.getOpPackageName();
    Log.i(TAG, "getLatestTetheringEntitlementResult caller:" + callerPkg);
    getConnector(c -> c.requestLatestTetheringEntitlementResult(type, receiver, showEntitlementUi, callerPkg));
}