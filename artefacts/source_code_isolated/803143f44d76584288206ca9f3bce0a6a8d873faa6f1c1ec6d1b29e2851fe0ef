public void manageRow(@NonNull NotificationEntry entry, @NonNull ExpandableNotificationRow row){
    mLogger.logManagedRow(entry.getKey());
    final BindEntry bindEntry = getBindEntry(entry);
    if (bindEntry == null) {
        return;
    }
    bindEntry.row = row;
    if (bindEntry.invalidated) {
        requestPipelineRun(entry);
    }
}