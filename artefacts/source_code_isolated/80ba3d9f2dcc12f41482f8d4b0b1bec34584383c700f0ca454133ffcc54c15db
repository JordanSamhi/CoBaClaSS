public String getImsi(){
    String operatorNumeric = ((TelephonyManager) mPhone.getContext().getSystemService(Context.TELEPHONY_SERVICE)).getSimOperatorNumericForPhone(mPhone.getPhoneId());
    if (!TextUtils.isEmpty(operatorNumeric) && getCdmaMin() != null) {
        return (operatorNumeric + getCdmaMin());
    } else {
        return null;
    }
}