public void onChange(boolean selfChange, @NonNull Collection<Uri> uris, @NotifyFlags int flags, @UserIdInt int userId){
    if (!CompatChanges.isChangeEnabled(ADD_CONTENT_OBSERVER_FLAGS) || android.os.Process.myUid() == android.os.Process.SYSTEM_UID) {
        onChange(selfChange, uris, userId);
    } else {
        onChange(selfChange, uris, flags);
    }
}