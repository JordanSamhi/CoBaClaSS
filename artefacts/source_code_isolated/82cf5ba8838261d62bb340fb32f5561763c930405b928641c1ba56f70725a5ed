public static void notifyUserOfConfigConversion(Context context){
    NotificationManager notificationManager = context.getSystemService(NotificationManager.class);
    NotificationChannel channel = new NotificationChannel(SystemNotificationChannels.NETWORK_STATUS, context.getString(com.android.internal.R.string.notification_channel_network_status), NotificationManager.IMPORTANCE_LOW);
    notificationManager.createNotificationChannel(channel);
    notificationManager.notify(SystemMessageProto.SystemMessage.NOTE_SOFTAP_CONFIG_CHANGED, createConversionNotification(context));
}