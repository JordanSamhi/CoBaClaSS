public void onSessionStarted(@Nullable ContentCaptureContext context, int sessionId, int uid, @NonNull IResultReceiver clientReceiver, int initialState){
    scheduleAsyncRequest((s) -> s.onSessionStarted(context, sessionId, uid, clientReceiver, initialState));
    writeSessionEvent(sessionId, FrameworkStatsLog.CONTENT_CAPTURE_SESSION_EVENTS__EVENT__ON_SESSION_STARTED, initialState, getComponentName(), context.getActivityComponent(), false);
}