public int getConfigInt(int item) throws ImsException{
    int ret = 0;
    try {
        ret = miConfig.getConfigInt(item);
    } catch (RemoteException e) {
        throw new ImsException("getInt()", e, ImsReasonInfo.CODE_LOCAL_SERVICE_UNAVAILABLE);
    }
    if (DBG)
        Rlog.d(TAG, "getInt(): item = " + item + ", ret =" + ret);
    return ret;
}