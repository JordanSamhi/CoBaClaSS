public String toString(){
    StringBuffer sb = new StringBuffer();
    sb.append("<LowpanCredential");
    if (isMasterKey()) {
        sb.append(" MasterKey");
        if (mMasterKeyIndex != UNSPECIFIED_KEY_INDEX) {
            sb.append(", Index:").append(mMasterKeyIndex);
        }
    } else {
        sb.append(" empty");
    }
    sb.append(">");
    return sb.toString();
}