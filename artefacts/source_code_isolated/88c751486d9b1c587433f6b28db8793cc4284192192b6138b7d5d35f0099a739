public boolean canConnectTo5GInDsdsMode(){
    ITelephony telephony = getITelephony();
    if (telephony == null)
        return true;
    try {
        return telephony.canConnectTo5GInDsdsMode();
    } catch (RemoteException ex) {
        return true;
    } catch (NullPointerException ex) {
        return true;
    }
}