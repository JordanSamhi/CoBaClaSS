public static void printCapabilitiesFull(PrintWriter pw, @ProcessCapability int caps){
    printCapabilitiesSummary(pw, caps);
    final int remain = caps & ~(PROCESS_CAPABILITY_FOREGROUND_LOCATION | PROCESS_CAPABILITY_FOREGROUND_CAMERA | PROCESS_CAPABILITY_FOREGROUND_MICROPHONE);
    if (remain != 0) {
        pw.print('+');
        pw.print(remain);
    }
}