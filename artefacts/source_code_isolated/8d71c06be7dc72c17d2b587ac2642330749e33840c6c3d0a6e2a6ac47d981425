public static String alwaysOnTopToString(@AlwaysOnTop int alwaysOnTop){
    switch(alwaysOnTop) {
        case ALWAYS_ON_TOP_UNDEFINED:
            return "undefined";
        case ALWAYS_ON_TOP_ON:
            return "on";
        case ALWAYS_ON_TOP_OFF:
            return "off";
    }
    return String.valueOf(alwaysOnTop);
}