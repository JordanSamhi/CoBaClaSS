public void writeLocked(XmlSerializer out) throws IOException{
    out.startTag(null, TAG_PACKAGE);
    out.attribute(null, ATTR_NAME, getName());
    out.attribute(null, ATTR_DURATION, String.valueOf(mDurationMs));
    out.attribute(null, ATTR_EXPLICIT_HEALTH_CHECK_DURATION, String.valueOf(mHealthCheckDurationMs));
    out.attribute(null, ATTR_PASSED_HEALTH_CHECK, String.valueOf(mHasPassedHealthCheck));
    out.endTag(null, TAG_PACKAGE);
}