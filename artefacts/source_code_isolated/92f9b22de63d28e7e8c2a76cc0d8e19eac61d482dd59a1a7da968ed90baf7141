public void readAbsolute(Callback<T> cb){
    if (!mThrottle) {
        readAbsoluteImpl(cb);
        return;
    }
    final long currTimeMs = SystemClock.elapsedRealtime();
    if (currTimeMs < mLastReadTimeMs + mMinTimeBetweenRead) {
        if (DEBUG) {
            Slog.d(mTag, "Throttle readAbsolute");
        }
        return;
    }
    readAbsoluteImpl(cb);
    mLastReadTimeMs = currTimeMs;
}