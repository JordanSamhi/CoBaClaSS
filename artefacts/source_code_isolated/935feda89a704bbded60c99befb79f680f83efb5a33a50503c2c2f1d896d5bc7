public boolean equals(Object obj){
    if (obj == null) {
        return false;
    }
    if (this == obj) {
        return true;
    }
    if (!(obj instanceof DecimalFormat)) {
        return false;
    }
    DecimalFormat other = (DecimalFormat) obj;
    return icuDecimalFormat.equals(other.icuDecimalFormat) && compareIcuRoundingIncrement(other.icuDecimalFormat);
}