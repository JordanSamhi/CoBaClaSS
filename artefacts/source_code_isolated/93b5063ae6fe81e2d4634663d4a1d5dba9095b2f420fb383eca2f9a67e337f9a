public void smartSuggestionsAdded(final NotificationEntry entry, int replyCount, int actionCount, boolean generatedByAssistant, boolean editBeforeSending){
    try {
        mBarService.onNotificationSmartSuggestionsAdded(entry.getSbn().getKey(), replyCount, actionCount, generatedByAssistant, editBeforeSending);
    } catch (RemoteException e) {
    }
}