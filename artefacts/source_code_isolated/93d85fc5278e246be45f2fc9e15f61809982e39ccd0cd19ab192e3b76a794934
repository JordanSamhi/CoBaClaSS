public void lookupAndFillInResourceNames(@NonNull Resources res){
    if ((mTitleResId == 0) && (mTextResId == 0) && (mDisabledMessageResId == 0) && (mIconResId == 0)) {
        return;
    }
    mTitleResName = lookUpResourceName(res, mTitleResId, false, mPackageName);
    mTextResName = lookUpResourceName(res, mTextResId, false, mPackageName);
    mDisabledMessageResName = lookUpResourceName(res, mDisabledMessageResId, false, mPackageName);
    mIconResName = lookUpResourceName(res, mIconResId, true, mPackageName);
}