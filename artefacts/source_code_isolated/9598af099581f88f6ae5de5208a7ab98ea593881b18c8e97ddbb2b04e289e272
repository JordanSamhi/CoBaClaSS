public String toString(){
    return new StringBuilder().append(this.getClass().getName()).append(" :{").append(" maxDataCalls = " + maxDataCalls).append(" isDcNrRestricted = " + isDcNrRestricted).append(" isNrAvailable = " + isNrAvailable).append(" isEnDcAvailable = " + isEnDcAvailable).append(" " + mLteVopsSupportInfo.toString()).append(" mIsUsingCarrierAggregation = " + mIsUsingCarrierAggregation).append(" }").toString();
}