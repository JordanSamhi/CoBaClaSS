public boolean equals(Object obj){
    if (obj instanceof NetworkPolicy) {
        final NetworkPolicy other = (NetworkPolicy) obj;
        return warningBytes == other.warningBytes && limitBytes == other.limitBytes && lastWarningSnooze == other.lastWarningSnooze && lastLimitSnooze == other.lastLimitSnooze && lastRapidSnooze == other.lastRapidSnooze && metered == other.metered && inferred == other.inferred && Objects.equals(template, other.template) && Objects.equals(cycleRule, other.cycleRule);
    }
    return false;
}