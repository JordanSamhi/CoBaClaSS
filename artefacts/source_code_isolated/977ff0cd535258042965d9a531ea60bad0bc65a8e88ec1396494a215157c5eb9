public static InputStream convert(){
    File file = new File(getLegacyWifiSharedDirectory(), LEGACY_AP_CONFIG_FILE);
    FileInputStream fis = null;
    try {
        fis = new FileInputStream(file);
    } catch (FileNotFoundException e) {
        return null;
    }
    if (fis == null)
        return null;
    return convert(fis);
}