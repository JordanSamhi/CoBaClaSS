public static MediaParser create(@NonNull OutputConsumer outputConsumer, @NonNull @ParserName String... parserNames){
    assertValidNames(parserNames);
    if (parserNames.length == 0) {
        parserNames = EXTRACTOR_FACTORIES_BY_NAME.keySet().toArray(new String[0]);
    }
    return new MediaParser(outputConsumer, true, parserNames);
}