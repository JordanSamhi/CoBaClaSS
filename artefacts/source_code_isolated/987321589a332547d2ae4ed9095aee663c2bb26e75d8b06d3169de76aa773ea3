public int getAttribution(Activity activity){
    if (activity == null) {
        return SettingsEnums.PAGE_UNKNOWN;
    }
    final Intent intent = activity.getIntent();
    if (intent == null) {
        return SettingsEnums.PAGE_UNKNOWN;
    }
    return intent.getIntExtra(EXTRA_SOURCE_METRICS_CATEGORY, SettingsEnums.PAGE_UNKNOWN);
}