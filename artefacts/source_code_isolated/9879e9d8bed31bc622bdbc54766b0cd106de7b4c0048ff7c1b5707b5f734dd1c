public boolean canSupportNetworkType(@NetworkType int networkType){
    if (networkType == TelephonyManager.NETWORK_TYPE_GSM && (mNetworkTypeBitmask & (TelephonyManager.NETWORK_TYPE_BITMASK_GPRS | TelephonyManager.NETWORK_TYPE_BITMASK_EDGE)) != 0) {
        return true;
    }
    return ServiceState.bitmaskHasTech(mNetworkTypeBitmask, networkType);
}