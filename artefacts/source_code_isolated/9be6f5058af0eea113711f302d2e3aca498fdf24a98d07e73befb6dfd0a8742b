public String toString(){
    return new StringBuilder("SubscriptionPlan{").append("cycleRule=").append(cycleRule).append(" title=").append(title).append(" summary=").append(summary).append(" dataLimitBytes=").append(dataLimitBytes).append(" dataLimitBehavior=").append(dataLimitBehavior).append(" dataUsageBytes=").append(dataUsageBytes).append(" dataUsageTime=").append(dataUsageTime).append(" networkTypes=").append(Arrays.toString(networkTypes)).append("}").toString();
}