public static String audioSystemErrorToString(@AudioSystemError int error){
    switch(error) {
        case SUCCESS:
            return "SUCCESS";
        case ERROR:
            return "ERROR";
        case BAD_VALUE:
            return "BAD_VALUE";
        case INVALID_OPERATION:
            return "INVALID_OPERATION";
        case PERMISSION_DENIED:
            return "PERMISSION_DENIED";
        case NO_INIT:
            return "NO_INIT";
        case DEAD_OBJECT:
            return "DEAD_OBJECT";
        case WOULD_BLOCK:
            return "WOULD_BLOCK";
        default:
            return ("unknown error:" + error);
    }
}