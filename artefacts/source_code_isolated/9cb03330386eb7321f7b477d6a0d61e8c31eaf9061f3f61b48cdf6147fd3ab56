public int firstIndexOnOrAfter(long timeStamp){
    final int size = mEvents.size();
    int result = size;
    int lo = 0;
    int hi = size - 1;
    while (lo <= hi) {
        final int mid = (lo + hi) >>> 1;
        final long midTimeStamp = mEvents.get(mid).mTimeStamp;
        if (midTimeStamp >= timeStamp) {
            hi = mid - 1;
            result = mid;
        } else {
            lo = mid + 1;
        }
    }
    return result;
}