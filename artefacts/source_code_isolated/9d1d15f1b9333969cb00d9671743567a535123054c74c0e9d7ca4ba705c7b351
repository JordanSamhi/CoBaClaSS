public boolean hasAudioTerminal(int subType){
    for (UsbDescriptor descriptor : mDescriptors) {
        if (descriptor instanceof UsbACInterface) {
            if (((UsbACInterface) descriptor).getSubclass() == UsbDescriptor.AUDIO_AUDIOCONTROL && ((UsbACInterface) descriptor).getSubtype() == subType) {
                return true;
            }
        }
    }
    return false;
}