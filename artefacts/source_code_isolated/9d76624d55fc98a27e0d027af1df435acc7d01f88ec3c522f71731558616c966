public static void wipeDetected(String filename, String reason){
    wtfAsSystemServer(TAG, "DB wipe detected:" + " package=" + ActivityThread.currentPackageName() + " reason=" + reason + " file=" + filename + " " + getFileTimestamps(filename) + " checkfile " + getFileTimestamps(filename + SQLiteGlobal.WIPE_CHECK_FILE_SUFFIX), new Throwable("STACKTRACE"));
}