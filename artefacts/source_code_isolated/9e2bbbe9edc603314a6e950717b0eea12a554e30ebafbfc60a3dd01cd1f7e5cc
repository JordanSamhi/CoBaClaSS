public boolean equals(Object obj){
    if (obj == this) {
        return true;
    }
    if (!(obj instanceof RC5ParameterSpec)) {
        return false;
    }
    RC5ParameterSpec other = (RC5ParameterSpec) obj;
    return ((version == other.version) && (rounds == other.rounds) && (wordSize == other.wordSize) && java.util.Arrays.equals(iv, other.iv));
}