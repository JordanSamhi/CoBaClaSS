public boolean equals(@NonNull Object o){
    if (this == o)
        return true;
    if (o == null || getClass() != o.getClass())
        return false;
    AudioVolumeGroup thatAvg = (AudioVolumeGroup) o;
    return mName == thatAvg.mName && mId == thatAvg.mId && mAudioAttributes.equals(thatAvg.mAudioAttributes);
}