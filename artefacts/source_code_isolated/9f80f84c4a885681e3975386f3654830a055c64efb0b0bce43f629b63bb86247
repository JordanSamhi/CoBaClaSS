public int getActiveSubInfoCount(String callingPackage, String callingFeatureId){
    List<SubscriptionInfo> records = getActiveSubscriptionInfoList(callingPackage, callingFeatureId);
    if (records == null) {
        if (VDBG)
            logd("[getActiveSubInfoCount] records null");
        return 0;
    }
    if (VDBG)
        logd("[getActiveSubInfoCount]- count: " + records.size());
    return records.size();
}