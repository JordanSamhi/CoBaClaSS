public void setPasspointMeteredOverride(String fqdn, int meteredOverride){
    enforceNetworkSettingsPermission();
    if (fqdn == null) {
        throw new IllegalArgumentException("FQDN cannot be null");
    }
    int callingUid = Binder.getCallingUid();
    mLog.info("setPasspointMeteredOverride=% uid=%").c(meteredOverride).c(callingUid).flush();
    mWifiThreadRunner.post(() -> mPasspointManager.setMeteredOverride(fqdn, meteredOverride));
}