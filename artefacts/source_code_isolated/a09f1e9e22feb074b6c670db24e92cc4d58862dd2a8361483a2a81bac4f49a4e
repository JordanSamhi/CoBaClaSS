public String getIsimDomain(int subId){
    return callPhoneMethodForSubIdWithPrivilegedCheck(subId, "getIsimDomain", (phone) -> {
        IsimRecords isim = phone.getIsimRecords();
        if (isim != null) {
            return isim.getIsimDomain();
        } else {
            return null;
        }
    });
}