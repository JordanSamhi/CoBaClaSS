public boolean equals(Object obj){
    if (this == obj)
        return true;
    if (obj instanceof EllipticCurve) {
        EllipticCurve curve = (EllipticCurve) obj;
        if ((field.equals(curve.field)) && (a.equals(curve.a)) && (b.equals(curve.b))) {
            return true;
        }
    }
    return false;
}