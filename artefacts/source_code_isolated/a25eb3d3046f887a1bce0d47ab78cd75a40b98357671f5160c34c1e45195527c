public boolean equals(Object obj){
    if (!(obj instanceof LowpanIdentity)) {
        return false;
    }
    LowpanIdentity rhs = (LowpanIdentity) obj;
    return Arrays.equals(mRawName, rhs.mRawName) && Arrays.equals(mXpanid, rhs.mXpanid) && mType.equals(rhs.mType) && mPanid == rhs.mPanid && mChannel == rhs.mChannel;
}