public void writeOnImsCallStart(int phoneId, ImsCallSession session){
    InProgressCallSession callSession = startNewCallSessionIfNeeded(phoneId);
    callSession.addEvent(new CallSessionEventBuilder(TelephonyCallSession.Event.Type.IMS_COMMAND).setCallIndex(getCallId(session)).setImsCommand(TelephonyCallSession.Event.ImsCommand.IMS_CMD_START));
}