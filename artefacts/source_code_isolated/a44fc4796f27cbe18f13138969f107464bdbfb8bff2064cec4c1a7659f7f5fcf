public int setConfig(int item, int value) throws ImsException{
    int ret = OperationStatusConstants.UNKNOWN;
    if (DBG) {
        Rlog.d(TAG, "setConfig(): item = " + item + "value = " + value);
    }
    try {
        ret = miConfig.setConfigInt(item, value);
    } catch (RemoteException e) {
        throw new ImsException("setConfig()", e, ImsReasonInfo.CODE_LOCAL_SERVICE_UNAVAILABLE);
    }
    if (DBG) {
        Rlog.d(TAG, "setConfig(): item = " + item + " value = " + value + " ret = " + ret);
    }
    return ret;
}