public boolean equals(Object obj){
    if (this == obj)
        return true;
    if (!(obj instanceof Chunk))
        return false;
    Chunk other = (Chunk) obj;
    if (mPurge != other.mPurge)
        return false;
    if (mComplete != other.mComplete)
        return false;
    if (!Objects.equals(mModified, other.mModified))
        return false;
    if (!Objects.equals(mRemoved, other.mRemoved))
        return false;
    return true;
}