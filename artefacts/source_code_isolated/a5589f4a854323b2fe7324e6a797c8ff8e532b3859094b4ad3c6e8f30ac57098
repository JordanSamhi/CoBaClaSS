public boolean equals(@Nullable Object o){
    if (this == o)
        return true;
    if (o == null || getClass() != o.getClass())
        return false;
    SnoozeCriterion that = (SnoozeCriterion) o;
    if (mId != null ? !mId.equals(that.mId) : that.mId != null)
        return false;
    if (mExplanation != null ? !mExplanation.equals(that.mExplanation) : that.mExplanation != null) {
        return false;
    }
    return mConfirmation != null ? mConfirmation.equals(that.mConfirmation) : that.mConfirmation == null;
}