public boolean equals(Object o){
    AvailableNetworkInfo ani;
    try {
        ani = (AvailableNetworkInfo) o;
    } catch (ClassCastException ex) {
        return false;
    }
    if (o == null) {
        return false;
    }
    return (mSubId == ani.mSubId && mPriority == ani.mPriority && (((mMccMncs != null) && mMccMncs.equals(ani.mMccMncs))) && mBands.equals(ani.mBands));
}