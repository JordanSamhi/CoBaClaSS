public synchronized void dump(){
    for (int key : mObjects.keySet()) {
        MtpObject obj = mObjects.get(key);
        Log.i(TAG, key + " | " + (obj.getParent() == null ? obj.getParent().getId() : "null") + " | " + obj.getName() + " | " + (obj.isDir() ? "dir" : "obj") + " | " + (obj.isVisited() ? "v" : "nv") + " | " + obj.getState());
    }
}