public static boolean isTerminal(@EventType int eventType){
    switch(eventType) {
        case ACTION_OVERTYPE:
        case ACTION_COPY:
        case ACTION_PASTE:
        case ACTION_CUT:
        case ACTION_SHARE:
        case ACTION_SMART_SHARE:
        case ACTION_DRAG:
        case ACTION_ABANDON:
        case ACTION_OTHER:
            return true;
        default:
            return false;
    }
}