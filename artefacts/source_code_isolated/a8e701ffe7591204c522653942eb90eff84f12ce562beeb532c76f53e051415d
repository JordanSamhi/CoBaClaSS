public Builder addMatchingUsage(@AttributeUsage int usage){
    Preconditions.checkState(mUsageMatchType != MATCH_TYPE_EXCLUSIVE, ERROR_MESSAGE_MISMATCHED_RULES);
    mAudioMixingRuleBuilder.addRule(new AudioAttributes.Builder().setUsage(usage).build(), AudioMixingRule.RULE_MATCH_ATTRIBUTE_USAGE);
    mUsageMatchType = MATCH_TYPE_INCLUSIVE;
    return this;
}