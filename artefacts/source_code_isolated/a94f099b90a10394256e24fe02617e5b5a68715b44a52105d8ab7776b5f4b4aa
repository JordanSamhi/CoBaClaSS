public String toString(){
    return new StringBuilder(TAG).append(":{ mCi=").append(mCi).append(" mPci=").append(mPci).append(" mTac=").append(mTac).append(" mEarfcn=").append(mEarfcn).append(" mBands=").append(Arrays.toString(mBands)).append(" mBandwidth=").append(mBandwidth).append(" mMcc=").append(mMccStr).append(" mMnc=").append(mMncStr).append(" mAlphaLong=").append(mAlphaLong).append(" mAlphaShort=").append(mAlphaShort).append(" mAdditionalPlmns=").append(mAdditionalPlmns).append(" mCsgInfo=").append(mCsgInfo).append("}").toString();
}