public BroadcastDispatcher providesBroadcastDispatcher(Context context, @Background Looper backgroundLooper, @Background Executor backgroundExecutor, DumpManager dumpManager, BroadcastDispatcherLogger logger){
    BroadcastDispatcher bD = new BroadcastDispatcher(context, backgroundLooper, backgroundExecutor, dumpManager, logger);
    bD.initialize();
    return bD;
}