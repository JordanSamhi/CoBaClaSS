public void insert(@Nullable T object, int index){
    synchronized (mLock) {
        if (mOriginalValues != null) {
            mOriginalValues.add(index, object);
        } else {
            mObjects.add(index, object);
        }
        mObjectsFromResources = false;
    }
    if (mNotifyOnChange)
        notifyDataSetChanged();
}