public void setPhase2Method(int phase2Method){
    switch(phase2Method) {
        case Phase2.NONE:
        case Phase2.PAP:
        case Phase2.MSCHAP:
        case Phase2.MSCHAPV2:
        case Phase2.GTC:
        case Phase2.SIM:
        case Phase2.AKA:
        case Phase2.AKA_PRIME:
            mPhase2Method = phase2Method;
            break;
        default:
            throw new IllegalArgumentException("Unknown Phase 2 method");
    }
}