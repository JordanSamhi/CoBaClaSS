public UiccSlot getUiccSlotForPhone(int phoneId){
    synchronized (mLock) {
        if (isValidPhoneIndex(phoneId)) {
            int slotId = getSlotIdFromPhoneId(phoneId);
            if (isValidSlotIndex(slotId)) {
                return mUiccSlots[slotId];
            }
        }
        return null;
    }
}