public EntityConfig build(){
    return new EntityConfig(mIncludedTypes == null ? Collections.emptyList() : new ArrayList<>(mIncludedTypes), mExcludedTypes == null ? Collections.emptyList() : new ArrayList<>(mExcludedTypes), mHints == null ? Collections.emptyList() : Collections.unmodifiableList(new ArrayList<>(mHints)), mIncludeTypesFromTextClassifier);
}