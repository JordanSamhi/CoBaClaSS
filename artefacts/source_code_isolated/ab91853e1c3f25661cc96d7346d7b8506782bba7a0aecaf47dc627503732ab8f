public static boolean appearsLikeAndroidVCardQuotedPrintable(String value){
    final int remainder = (value.length() % 3);
    if (value.length() < 2 || (remainder != 1 && remainder != 0)) {
        return false;
    }
    for (int i = 0; i < value.length(); i += 3) {
        if (value.charAt(i) != '=') {
            return false;
        }
    }
    return true;
}