public PersistableBundle getCarrierConfig(final TetheringConfiguration config){
    final CarrierConfigManager configManager = (CarrierConfigManager) mContext.getSystemService(Context.CARRIER_CONFIG_SERVICE);
    if (configManager == null)
        return null;
    final PersistableBundle carrierConfig = configManager.getConfigForSubId(config.activeDataSubId);
    if (CarrierConfigManager.isConfigForIdentifiedCarrier(carrierConfig)) {
        return carrierConfig;
    }
    return null;
}