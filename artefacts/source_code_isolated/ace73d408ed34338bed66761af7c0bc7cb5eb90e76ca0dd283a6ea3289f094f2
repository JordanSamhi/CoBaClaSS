public int checkResultUsable(@IntRange(from = 0) int start, @IntRange(from = 0) int end, @NonNull TextDirectionHeuristic textDir, @NonNull TextPaint paint, @Layout.BreakStrategy int strategy, @Layout.HyphenationFrequency int frequency){
    if (mStart != start || mEnd != end) {
        return Params.UNUSABLE;
    } else {
        return mParams.checkResultUsable(paint, textDir, strategy, frequency);
    }
}