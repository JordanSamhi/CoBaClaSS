public boolean equals(final Object obj){
    if (obj == null) {
        return false;
    }
    if (this == obj) {
        return true;
    }
    if (obj instanceof MandatoryStreamCombination) {
        final MandatoryStreamCombination other = (MandatoryStreamCombination) obj;
        if ((mDescription != other.mDescription) || (mIsReprocessable != other.mIsReprocessable) || (mStreamsInformation.size() != other.mStreamsInformation.size())) {
            return false;
        }
        return mStreamsInformation.equals(other.mStreamsInformation);
    }
    return false;
}