public boolean update(String kvList){
    if (kvList == null || "".equals(kvList)) {
        return true;
    }
    if (!("," + kvList).matches(COMMA_KEY_VAL_STAR)) {
        return false;
    }
    loadResources(mContext);
    Values v = new Values(mVal);
    try {
        v.parseString(kvList);
        v.validate();
        mVal = v;
        return true;
    } catch (IllegalArgumentException e) {
        return false;
    }
}