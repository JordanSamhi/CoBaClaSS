public String toString(){
    StringBuffer sb = new StringBuffer();
    sb.append("[");
    for (int i = 0; i < mCount; i++) {
        final int index = (mStart + i) % mSize;
        final long time = mTimes[index];
        final float value = mValues[index];
        sb.append(value + " @ " + time);
        if (i + 1 != mCount) {
            sb.append(", ");
        }
    }
    sb.append("]");
    return sb.toString();
}