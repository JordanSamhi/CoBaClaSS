public Identifier[] getAllIds(@IdentifierType int type){
    List<Identifier> out = new ArrayList<>();
    if (mPrimaryId.getType() == type)
        out.add(mPrimaryId);
    for (Identifier id : mSecondaryIds) {
        if (id.getType() == type)
            out.add(id);
    }
    return out.toArray(new Identifier[out.size()]);
}