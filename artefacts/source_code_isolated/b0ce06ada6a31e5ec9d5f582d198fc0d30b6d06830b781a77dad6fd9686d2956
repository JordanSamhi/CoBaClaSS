public void migrateImsSettings(){
    migrateImsSettingHelper(Settings.Global.ENHANCED_4G_MODE_ENABLED, SubscriptionManager.ENHANCED_4G_MODE_ENABLED);
    migrateImsSettingHelper(Settings.Global.VT_IMS_ENABLED, SubscriptionManager.VT_IMS_ENABLED);
    migrateImsSettingHelper(Settings.Global.WFC_IMS_ENABLED, SubscriptionManager.WFC_IMS_ENABLED);
    migrateImsSettingHelper(Settings.Global.WFC_IMS_MODE, SubscriptionManager.WFC_IMS_MODE);
    migrateImsSettingHelper(Settings.Global.WFC_IMS_ROAMING_MODE, SubscriptionManager.WFC_IMS_ROAMING_MODE);
    migrateImsSettingHelper(Settings.Global.WFC_IMS_ROAMING_ENABLED, SubscriptionManager.WFC_IMS_ROAMING_ENABLED);
}