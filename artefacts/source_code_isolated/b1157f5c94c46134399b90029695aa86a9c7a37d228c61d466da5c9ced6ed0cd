public int predictTxThroughput(@NonNull WifiNative.ConnectionCapabilities capabilities, int rssiDbm, int frequency, int channelUtilization){
    int channelUtilizationFinal = getValidChannelUtilization(frequency, INVALID, channelUtilization, false);
    return predictThroughputInternal(capabilities.wifiStandard, capabilities.channelBandwidth, rssiDbm, capabilities.maxNumberTxSpatialStreams, channelUtilizationFinal);
}