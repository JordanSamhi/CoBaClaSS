public boolean equals(Object o){
    if (this == o) {
        return true;
    }
    if (o == null || getClass() != o.getClass()) {
        return false;
    }
    if (!super.equals(o)) {
        return false;
    }
    RectShape rectShape = (RectShape) o;
    return Objects.equals(mRect, rectShape.mRect);
}