public int getRawFixed32At(int pos){
    return (0x00ff & (int) mBuffers.get(pos / mChunkSize)[pos % mChunkSize]) | ((0x0ff & (int) mBuffers.get((pos + 1) / mChunkSize)[(pos + 1) % mChunkSize]) << 8) | ((0x0ff & (int) mBuffers.get((pos + 2) / mChunkSize)[(pos + 2) % mChunkSize]) << 16) | ((0x0ff & (int) mBuffers.get((pos + 3) / mChunkSize)[(pos + 3) % mChunkSize]) << 24);
}