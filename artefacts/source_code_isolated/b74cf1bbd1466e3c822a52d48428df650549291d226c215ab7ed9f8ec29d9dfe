public float getCoarseLocationAccuracyM(){
    long identity = Binder.clearCallingIdentity();
    try {
        return Settings.Secure.getFloat(mContext.getContentResolver(), LOCATION_COARSE_ACCURACY_M, DEFAULT_COARSE_LOCATION_ACCURACY_M);
    } finally {
        Binder.restoreCallingIdentity(identity);
    }
}