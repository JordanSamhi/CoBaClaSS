public static boolean isEffectSupportedForDevice(@NonNull UUID uuid, @NonNull AudioDeviceAttributes device){
    try {
        AudioEffect fx = new AudioEffect(EFFECT_TYPE_NULL, Objects.requireNonNull(uuid), 0, -2, Objects.requireNonNull(device), true);
        fx.release();
        return true;
    } catch (Exception e) {
        return false;
    }
}