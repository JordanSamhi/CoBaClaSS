public boolean equals(Object o){
    if (this == o)
        return true;
    if (!(o instanceof CapabilityChangeRequest))
        return false;
    CapabilityChangeRequest that = (CapabilityChangeRequest) o;
    if (!mCapabilitiesToEnable.equals(that.mCapabilitiesToEnable))
        return false;
    return mCapabilitiesToDisable.equals(that.mCapabilitiesToDisable);
}