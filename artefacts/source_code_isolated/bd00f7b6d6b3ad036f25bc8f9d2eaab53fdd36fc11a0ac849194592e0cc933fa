public List<String> getPackageNames(){
    List<String> pkgNames = new ArrayList<String>();
    if (mAccessRules != null) {
        for (UiccAccessRule ar : mAccessRules) {
            if (!TextUtils.isEmpty(ar.getPackageName())) {
                pkgNames.add(ar.getPackageName());
            }
        }
    }
    return pkgNames;
}