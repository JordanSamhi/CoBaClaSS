public boolean hasNextDouble(){
    setRadix(10);
    boolean result = hasNext(floatPattern());
    if (result) {
        try {
            String s = processFloatToken(hasNextResult);
            typeCache = Double.valueOf(Double.parseDouble(s));
        } catch (NumberFormatException nfe) {
            result = false;
        }
    }
    return result;
}