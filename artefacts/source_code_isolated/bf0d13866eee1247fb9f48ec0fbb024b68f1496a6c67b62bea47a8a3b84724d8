public String toString(){
    return "DataProfile=" + mProfileId + "/" + mProtocolType + "/" + mAuthType + "/" + (TelephonyUtils.IS_USER ? "***/***/***" : (mApn + "/" + mUserName + "/" + mPassword)) + "/" + mType + "/" + mMaxConnectionsTime + "/" + mMaxConnections + "/" + mWaitTime + "/" + mEnabled + "/" + mSupportedApnTypesBitmask + "/" + mRoamingProtocolType + "/" + mBearerBitmask + "/" + mMtuV4 + "/" + mMtuV6 + "/" + mPersistent + "/" + mPreferred;
}