public void transferWorkChains(WorkSource other){
    if (mChains != null) {
        mChains.clear();
    }
    if (other.mChains == null || other.mChains.isEmpty()) {
        return;
    }
    if (mChains == null) {
        mChains = new ArrayList<>(4);
    }
    mChains.addAll(other.mChains);
    other.mChains.clear();
}