public Builder setExcessPathLengthUncertaintyMeters(@FloatRange(from = 0.0f) float excessPathLengthUncertaintyMeters){
    mExcessPathLengthUncertaintyMeters = excessPathLengthUncertaintyMeters;
    mSingleSatCorrectionFlags = (byte) (mSingleSatCorrectionFlags | HAS_EXCESS_PATH_LENGTH_UNC_MASK);
    return this;
}