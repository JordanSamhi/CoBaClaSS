public void unloadNanoAppFromHub(int contextHubId, IContextHubTransactionCallback transactionCallback, long nanoAppId) throws RemoteException{
    checkPermissions();
    if (!checkHalProxyAndContextHubId(contextHubId, transactionCallback, ContextHubTransaction.TYPE_UNLOAD_NANOAPP)) {
        return;
    }
    ContextHubServiceTransaction transaction = mTransactionManager.createUnloadTransaction(contextHubId, nanoAppId, transactionCallback);
    mTransactionManager.addTransaction(transaction);
}