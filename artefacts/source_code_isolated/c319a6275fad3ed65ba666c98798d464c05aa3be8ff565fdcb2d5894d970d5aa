public String toString(){
    StringBuilder sb = new StringBuilder();
    sb.append("java.util.regex.Matcher");
    sb.append("[pattern=" + pattern());
    sb.append(" region=");
    sb.append(regionStart() + "," + regionEnd());
    sb.append(" lastmatch=");
    if (matchFound && (group() != null)) {
        sb.append(group());
    }
    sb.append("]");
    return sb.toString();
}