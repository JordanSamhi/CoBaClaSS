public boolean isAnyManagedProfilePublicMode(){
    synchronized (mLock) {
        for (int i = mCurrentManagedProfiles.size() - 1; i >= 0; i--) {
            if (isLockscreenPublicMode(mCurrentManagedProfiles.valueAt(i).id)) {
                return true;
            }
        }
    }
    return false;
}