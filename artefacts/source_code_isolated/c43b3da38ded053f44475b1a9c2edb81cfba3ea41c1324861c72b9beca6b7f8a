public Face toFace(int id, Point leftEyePosition, Point rightEyePosition, Point mouthPosition){
    int idSafe = clipLower(id, 0, rect, "id");
    int score = clip(weight, Face.SCORE_MIN, Face.SCORE_MAX, rect, "score");
    return new Face(rect, score, idSafe, leftEyePosition, rightEyePosition, mouthPosition);
}