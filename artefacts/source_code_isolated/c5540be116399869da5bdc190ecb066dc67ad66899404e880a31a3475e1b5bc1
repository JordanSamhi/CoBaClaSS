public boolean equals(@Nullable Object o){
    if (this == o)
        return true;
    if (o == null || getClass() != o.getClass())
        return false;
    @SuppressWarnings("unchecked")
    KeyphraseMetadata that = (KeyphraseMetadata) o;
    return true && mId == that.mId && java.util.Objects.equals(mKeyphrase, that.mKeyphrase) && java.util.Objects.equals(mSupportedLocales, that.mSupportedLocales) && mRecognitionModeFlags == that.mRecognitionModeFlags;
}