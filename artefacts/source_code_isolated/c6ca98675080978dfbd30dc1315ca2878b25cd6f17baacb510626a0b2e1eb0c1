public void extractInto(@Nullable WallpaperColors inWallpaperColors, @NonNull GradientColors outColorsNormal, @NonNull GradientColors outColorsDark, @NonNull GradientColors outColorsExtraDark){
    boolean success = runTonalExtraction(inWallpaperColors, outColorsNormal, outColorsDark, outColorsExtraDark);
    if (!success) {
        applyFallback(inWallpaperColors, outColorsNormal, outColorsDark, outColorsExtraDark);
    }
}