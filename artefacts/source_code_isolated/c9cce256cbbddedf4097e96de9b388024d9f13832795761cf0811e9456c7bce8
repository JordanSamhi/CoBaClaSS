public boolean equals(@Nullable Object obj){
    if (this == obj) {
        return true;
    }
    if (obj == null) {
        return false;
    }
    if (getClass() != obj.getClass()) {
        return false;
    }
    PinResult other = (PinResult) obj;
    return (mType == other.mType && mAttemptsRemaining == other.mAttemptsRemaining);
}