public boolean satisfiedByUids(@NonNull NetworkCapabilities nc){
    if (null == nc.mUids || null == mUids)
        return true;
    for (UidRange requiredRange : mUids) {
        if (requiredRange.contains(nc.mOwnerUid))
            return true;
        if (!nc.appliesToUidRange(requiredRange)) {
            return false;
        }
    }
    return true;
}