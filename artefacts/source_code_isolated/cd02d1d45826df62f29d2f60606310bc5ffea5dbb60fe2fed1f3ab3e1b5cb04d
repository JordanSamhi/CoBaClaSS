public static int countGsmSeptets(char c, boolean throwsException) throws EncodeException{
    if (sCharsToGsmTables[0].get(c, -1) != -1) {
        return 1;
    }
    if (sCharsToShiftTables[0].get(c, -1) != -1) {
        return 2;
    }
    if (throwsException) {
        throw new EncodeException(c);
    } else {
        return 1;
    }
}