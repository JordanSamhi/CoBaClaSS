public void write(byte[] buffer) throws IOException{
    if (mRemaining >= 0) {
        if (mRemaining == 0)
            return;
        int count = buffer.length;
        if (count > mRemaining)
            count = (int) mRemaining;
        super.write(buffer);
        mRemaining -= count;
        return;
    }
    super.write(buffer);
}