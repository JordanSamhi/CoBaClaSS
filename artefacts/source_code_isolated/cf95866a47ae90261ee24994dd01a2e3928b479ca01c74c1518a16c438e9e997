public int stop(){
    Log.d(TAG, "Write Stats Log for Playback.");
    FrameworkStatsLog.write(FrameworkStatsLog.TV_TUNER_DVR_STATUS, mUserId, FrameworkStatsLog.TV_TUNER_DVR_STATUS__TYPE__PLAYBACK, FrameworkStatsLog.TV_TUNER_DVR_STATUS__STATE__STOPPED, mSegmentId, mUnderflow);
    return nativeStopDvr();
}