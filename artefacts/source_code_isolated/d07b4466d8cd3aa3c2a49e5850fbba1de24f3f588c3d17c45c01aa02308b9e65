public String toString(){
    return "SmsCbMessage{geographicalScope=" + mGeographicalScope + ", serialNumber=" + mSerialNumber + ", location=" + mLocation + ", serviceCategory=" + mServiceCategory + ", language=" + mLanguage + ", body=" + mBody + ", priority=" + mPriority + (mEtwsWarningInfo != null ? (", " + mEtwsWarningInfo.toString()) : "") + (mCmasWarningInfo != null ? (", " + mCmasWarningInfo.toString()) : "") + ", maximumWaitingTime=" + mMaximumWaitTimeSec + ", received time=" + mReceivedTimeMillis + ", slotIndex = " + mSlotIndex + ", geo=" + (mGeometries != null ? CbGeoUtils.encodeGeometriesToString(mGeometries) : "null") + '}';
}