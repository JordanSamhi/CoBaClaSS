public static void RGBToHSV(@IntRange(from = 0, to = 255) int red, @IntRange(from = 0, to = 255) int green, @IntRange(from = 0, to = 255) int blue, @Size(3) float[] hsv){
    if (hsv.length < 3) {
        throw new RuntimeException("3 components required for hsv");
    }
    nativeRGBToHSV(red, green, blue, hsv);
}