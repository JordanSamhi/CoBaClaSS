public void dump(@NonNull String prefix, @NonNull PrintWriter pw){
    synchronized (this) {
        pw.print(prefix);
        pw.print("destroyed: ");
        pw.println(mDestroyed);
        pw.print(prefix);
        pw.print("updateCalled: ");
        pw.println(mUpdateCalled);
        if (mFillView != null) {
            pw.print(prefix);
            pw.print("fill window: ");
            pw.println(mShowing ? "shown" : "hidden");
            pw.print(prefix);
            pw.print("fill view: ");
            pw.println(mFillView);
            pw.print(prefix);
            pw.print("mBounds: ");
            pw.println(mBounds);
            pw.print(prefix);
            pw.print("mWm: ");
            pw.println(mWm);
        }
    }
}