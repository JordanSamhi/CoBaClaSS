public String toString(){
    return new StringBuffer("ResponderConfig: macAddress=").append(macAddress).append(", peerHandle=").append(peerHandle == null ? "<null>" : peerHandle.peerId).append(", responderType=").append(responderType).append(", supports80211mc=").append(supports80211mc).append(", channelWidth=").append(channelWidth).append(", frequency=").append(frequency).append(", centerFreq0=").append(centerFreq0).append(", centerFreq1=").append(centerFreq1).append(", preamble=").append(preamble).toString();
}