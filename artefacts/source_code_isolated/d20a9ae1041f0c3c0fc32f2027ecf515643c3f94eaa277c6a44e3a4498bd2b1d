public void retrieveNotification(String cardId, int seqNumber, @CallbackExecutor Executor executor, ResultCallback<EuiccNotification> callback){
    try {
        getIEuiccCardController().retrieveNotification(mContext.getOpPackageName(), cardId, seqNumber, new IRetrieveNotificationCallback.Stub() {

            @Override
            public void onComplete(int resultCode, EuiccNotification notification) {
                final long token = Binder.clearCallingIdentity();
                try {
                    executor.execute(() -> callback.onComplete(resultCode, notification));
                } finally {
                    Binder.restoreCallingIdentity(token);
                }
            }
        });
    } catch (RemoteException e) {
        Log.e(TAG, "Error calling retrieveNotification", e);
        throw e.rethrowFromSystemServer();
    }
}