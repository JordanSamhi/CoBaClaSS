public int requestBackup(@UserIdInt int userId, String[] packages, IBackupObserver observer, IBackupManagerMonitor monitor, int flags){
    UserBackupManagerService userBackupManagerService = getServiceForUserIfCallerHasPermission(userId, "requestBackup()");
    return userBackupManagerService == null ? BackupManager.ERROR_BACKUP_NOT_ALLOWED : userBackupManagerService.requestBackup(packages, observer, monitor, flags);
}