public void close(){
    synchronized (mLockObj) {
        if (mSession != null) {
            mSession.close();
            mSession = null;
        } else {
            logi("close :: Cannot close Null call session!");
        }
        mCallProfile = null;
        mProposedCallProfile = null;
        mLastReasonInfo = null;
        mMediaSession = null;
    }
}