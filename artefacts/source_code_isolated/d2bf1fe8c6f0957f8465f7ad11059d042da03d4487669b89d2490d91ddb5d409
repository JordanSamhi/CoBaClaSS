public static String serviceTypeToString(@ServiceType int serviceType){
    switch(serviceType) {
        case SERVICE_TYPE_VOICE:
            return "VOICE";
        case SERVICE_TYPE_DATA:
            return "DATA";
        case SERVICE_TYPE_SMS:
            return "SMS";
        case SERVICE_TYPE_VIDEO:
            return "VIDEO";
        case SERVICE_TYPE_EMERGENCY:
            return "EMERGENCY";
    }
    return "Unknown service type " + serviceType;
}