public void filterSms(@NonNull MessagePdu pdu, @NonNull String format, int destPort, int subId, @NonNull final CarrierMessagingCallbackWrapper callback){
    if (mICarrierMessagingService != null) {
        try {
            mICarrierMessagingService.filterSms(pdu, format, destPort, subId, new CarrierMessagingCallbackWrapperInternal(callback));
        } catch (RemoteException e) {
            throw new RuntimeException(e);
        }
    }
}