public boolean equals(@Nullable Object o){
    if (this == o)
        return true;
    if (o == null || getClass() != o.getClass())
        return false;
    @SuppressWarnings("unchecked")
    Association that = (Association) o;
    return true && userId == that.userId && Objects.equals(deviceAddress, that.deviceAddress) && Objects.equals(companionAppPackage, that.companionAppPackage);
}