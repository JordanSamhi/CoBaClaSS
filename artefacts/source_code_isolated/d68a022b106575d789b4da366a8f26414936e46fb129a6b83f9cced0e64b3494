public File[] listFiles(FilenameFilter filter){
    String[] ss = list();
    if (ss == null)
        return null;
    ArrayList<File> files = new ArrayList<>();
    for (String s : ss) if ((filter == null) || filter.accept(this, s))
        files.add(new File(s, this));
    return files.toArray(new File[files.size()]);
}