public Builder setValue(@NonNull AutofillId id, @Nullable AutofillValue value, @NonNull RemoteViews presentation, @NonNull InlinePresentation inlinePresentation){
    throwIfDestroyed();
    Preconditions.checkNotNull(presentation, "presentation cannot be null");
    Preconditions.checkNotNull(inlinePresentation, "inlinePresentation cannot be null");
    setLifeTheUniverseAndEverything(id, value, presentation, inlinePresentation, null);
    return this;
}