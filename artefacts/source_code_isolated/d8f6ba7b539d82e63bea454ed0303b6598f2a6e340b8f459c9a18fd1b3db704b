public boolean isDataAllowedInVoiceCall(){
    try {
        ITelephony service = getITelephony();
        if (service != null) {
            return service.isDataAllowedInVoiceCall(getSubId());
        }
    } catch (RemoteException ex) {
        if (!isSystemProcess()) {
            ex.rethrowAsRuntimeException();
        }
    }
    return false;
}