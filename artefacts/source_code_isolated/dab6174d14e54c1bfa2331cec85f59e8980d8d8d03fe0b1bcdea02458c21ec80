public boolean contains(DeviceFilter device){
    if (mVendorId != -1 && device.mVendorId != mVendorId)
        return false;
    if (mProductId != -1 && device.mProductId != mProductId)
        return false;
    if (mManufacturerName != null && !Objects.equals(mManufacturerName, device.mManufacturerName)) {
        return false;
    }
    if (mProductName != null && !Objects.equals(mProductName, device.mProductName)) {
        return false;
    }
    if (mSerialNumber != null && !Objects.equals(mSerialNumber, device.mSerialNumber)) {
        return false;
    }
    return matches(device.mClass, device.mSubclass, device.mProtocol);
}