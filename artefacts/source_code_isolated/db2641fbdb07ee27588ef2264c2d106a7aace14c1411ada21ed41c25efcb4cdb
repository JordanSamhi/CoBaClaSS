public String toString(){
    return new StringBuilder().append("{mConnectionStatus=").append(getConnectionStatusString()).append(",mCellBandwidthDownlinkKhz=").append(mCellBandwidthDownlinkKhz).append(",mRat=").append(TelephonyManager.getNetworkTypeName(mRat)).append(",mFrequencyRange=").append(ServiceState.frequencyRangeToString(mFrequencyRange)).append(",mChannelNumber=").append(mChannelNumber).append(",mContextIds=").append(Arrays.toString(mContextIds)).append(",mPhysicalCellId=").append(mPhysicalCellId).append("}").toString();
}