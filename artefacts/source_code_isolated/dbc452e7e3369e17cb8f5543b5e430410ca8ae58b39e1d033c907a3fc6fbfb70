public boolean equals(Object o){
    if (this == o) {
        return true;
    }
    if (o == null || getClass() != o.getClass()) {
        return false;
    }
    DistroFormatVersion that = (DistroFormatVersion) o;
    if (mMajorVersion != that.mMajorVersion) {
        return false;
    }
    return mMinorVersion == that.mMinorVersion;
}