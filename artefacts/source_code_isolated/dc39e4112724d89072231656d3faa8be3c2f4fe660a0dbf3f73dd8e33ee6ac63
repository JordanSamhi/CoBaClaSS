public static String getResourceTypeName(@NonNull String fullResourceName){
    final int p1 = fullResourceName.indexOf(':');
    if (p1 < 0) {
        return null;
    }
    final int p2 = fullResourceName.indexOf('/', p1 + 1);
    if (p2 < 0) {
        return null;
    }
    return fullResourceName.substring(p1 + 1, p2);
}