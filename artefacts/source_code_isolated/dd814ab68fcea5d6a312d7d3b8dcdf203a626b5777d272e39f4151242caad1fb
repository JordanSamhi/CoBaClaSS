public static boolean scheduleEquals(@Nullable WifiNative.ScanSettings schedule1, @Nullable WifiNative.ScanSettings schedule2){
    if (schedule1 == null || schedule2 == null)
        return false;
    if (schedule1 == schedule2)
        return true;
    if (schedule1.base_period_ms != schedule2.base_period_ms)
        return false;
    if (schedule1.max_ap_per_scan != schedule2.max_ap_per_scan)
        return false;
    if (schedule1.report_threshold_percent != schedule2.report_threshold_percent)
        return false;
    if (schedule1.report_threshold_num_scans != schedule2.report_threshold_num_scans) {
        return false;
    }
    if (schedule1.num_buckets != schedule2.num_buckets)
        return false;
    for (int b = 0; b < schedule1.num_buckets; b++) {
        if (!bucketEquals(schedule1.buckets[b], schedule2.buckets[b])) {
            return false;
        }
    }
    return true;
}