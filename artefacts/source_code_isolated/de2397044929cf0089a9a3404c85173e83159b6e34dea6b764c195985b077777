public static boolean isAtLeastStrength(int sensorStrength, int requestedStrength){
    sensorStrength &= Authenticators.BIOMETRIC_MIN_STRENGTH;
    if ((sensorStrength & ~requestedStrength) != 0) {
        return false;
    }
    for (int i = Authenticators.BIOMETRIC_MAX_STRENGTH; i <= requestedStrength; i = (i << 1) | 1) {
        if (i == sensorStrength) {
            return true;
        }
    }
    Slog.e(BiometricService.TAG, "Unknown sensorStrength: " + sensorStrength + ", requestedStrength: " + requestedStrength);
    return false;
}