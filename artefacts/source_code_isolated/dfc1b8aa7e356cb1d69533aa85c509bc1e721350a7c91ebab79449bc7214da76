public boolean run(@NonNull Runnable runnable){
    boolean runWithScissorsSuccess = runWithScissors(mHandler, runnable, RUN_WITH_SCISSORS_TIMEOUT_MILLIS);
    if (runWithScissorsSuccess) {
        return true;
    } else {
        Log.e(TAG, "WifiThreadRunner.run() timed out!", new Throwable("Stack trace:"));
        return false;
    }
}