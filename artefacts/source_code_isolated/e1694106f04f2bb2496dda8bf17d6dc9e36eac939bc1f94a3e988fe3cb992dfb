public Builder setFieldInlinePresentation(@NonNull AutofillId id, @Nullable AutofillValue value, @Nullable Pattern filter, @NonNull InlinePresentation inlinePresentation){
    throwIfDestroyed();
    Preconditions.checkNotNull(inlinePresentation, "inlinePresentation cannot be null");
    setLifeTheUniverseAndEverything(id, value, null, inlinePresentation, new DatasetFieldFilter(filter));
    return this;
}