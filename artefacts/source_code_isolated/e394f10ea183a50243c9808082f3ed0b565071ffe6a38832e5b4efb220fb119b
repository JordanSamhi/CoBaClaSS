public boolean setLowLatencyMode(boolean enabled){
    if (mVerboseLoggingEnabled) {
        Log.d(TAG, "Setting low latency mode to " + enabled);
    }
    if (!mWifiNative.setLowLatencyMode(enabled)) {
        Log.e(TAG, "Failed to setLowLatencyMode");
        return false;
    }
    return true;
}