public void reinit(long startTime, long startElapsedTime, @NonNull IBinder clientId, @NonNull Runnable onDeath, int uidState) throws RemoteException{
    mStartTime = startTime;
    mStartElapsedTime = startElapsedTime;
    mClientId = clientId;
    mOnDeath = onDeath;
    mUidState = uidState;
    clientId.linkToDeath(this, 0);
}