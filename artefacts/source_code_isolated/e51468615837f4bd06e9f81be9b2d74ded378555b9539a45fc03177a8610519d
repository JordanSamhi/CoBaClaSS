public Transaction deferTransactionUntilSurface(SurfaceControl sc, Surface barrierSurface, long frameNumber){
    if (frameNumber < 0) {
        return this;
    }
    checkPreconditions(sc);
    nativeDeferTransactionUntilSurface(mNativeObject, sc.mNativeObject, barrierSurface.mNativeObject, frameNumber);
    return this;
}