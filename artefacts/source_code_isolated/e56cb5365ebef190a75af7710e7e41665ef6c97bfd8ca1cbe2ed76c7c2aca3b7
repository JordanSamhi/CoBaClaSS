public void setDeviceVolumeBehavior(@NonNull AudioDeviceAttributes device, @DeviceVolumeBehavior int deviceVolumeBehavior){
    Objects.requireNonNull(device);
    enforceValidVolumeBehavior(deviceVolumeBehavior);
    final IAudioService service = getService();
    try {
        service.setDeviceVolumeBehavior(device, deviceVolumeBehavior, mApplicationContext.getOpPackageName());
    } catch (RemoteException e) {
        throw e.rethrowFromSystemServer();
    }
}