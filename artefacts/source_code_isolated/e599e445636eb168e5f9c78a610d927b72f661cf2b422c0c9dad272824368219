public int hashCode(){
    int result = Objects.hash(mId, mType, mLanguage, mDescription);
    if (mType == TYPE_AUDIO) {
        result = Objects.hash(result, mAudioChannelCount, mAudioSampleRate);
    } else if (mType == TYPE_VIDEO) {
        result = Objects.hash(result, mVideoWidth, mVideoHeight, mVideoFrameRate, mVideoPixelAspectRatio);
    }
    return result;
}