public Builder setProbabilityLineOfSight(@FloatRange(from = 0.0f, to = 1.0f) float probSatIsLos){
    Preconditions.checkArgumentInRange(probSatIsLos, 0, 1, "probSatIsLos should be between 0 and 1.");
    mProbSatIsLos = probSatIsLos;
    mSingleSatCorrectionFlags = (byte) (mSingleSatCorrectionFlags | HAS_PROB_SAT_IS_LOS_MASK);
    return this;
}