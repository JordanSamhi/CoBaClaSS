public void setRemoteInput(RemoteInput[] remoteInputs, RemoteInput remoteInput, @Nullable EditedSuggestionInfo editedSuggestionInfo){
    mRemoteInputs = remoteInputs;
    mRemoteInput = remoteInput;
    mEditText.setHint(mRemoteInput.getLabel());
    mEntry.editedSuggestionInfo = editedSuggestionInfo;
    if (editedSuggestionInfo != null) {
        mEntry.remoteInputText = editedSuggestionInfo.originalText;
    }
}