public void setChannelMask(@Nullable Collection<Integer> mask){
    if (mask == null) {
        mChannelMask = null;
    } else {
        if (mChannelMask == null) {
            mChannelMask = new ArrayList<>();
        } else {
            mChannelMask.clear();
        }
        mChannelMask.addAll(mask);
    }
}