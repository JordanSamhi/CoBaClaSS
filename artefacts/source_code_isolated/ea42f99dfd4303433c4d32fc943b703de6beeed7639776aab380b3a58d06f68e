public boolean isTheFinalCountDown(){
    try {
        getContext().startActivity(new Intent(Intent.ACTION_VIEW, Uri.parse("https://youtu.be/9jK-NcRmVcw")).addCategory(Intent.CATEGORY_BROWSABLE).addFlags(Intent.FLAG_ACTIVITY_NEW_DOCUMENT | Intent.FLAG_ACTIVITY_LAUNCH_ADJACENT));
        return true;
    } catch (Exception e) {
        return false;
    }
}