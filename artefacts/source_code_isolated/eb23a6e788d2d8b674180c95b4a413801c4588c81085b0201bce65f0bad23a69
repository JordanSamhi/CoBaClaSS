public void addConfigCallback(IImsConfigCallback callback) throws ImsException{
    if (DBG)
        Rlog.d(TAG, "addConfigCallback: " + callback);
    try {
        miConfig.addImsConfigCallback(callback);
    } catch (RemoteException e) {
        throw new ImsException("addConfigCallback()", e, ImsReasonInfo.CODE_LOCAL_SERVICE_UNAVAILABLE);
    }
}