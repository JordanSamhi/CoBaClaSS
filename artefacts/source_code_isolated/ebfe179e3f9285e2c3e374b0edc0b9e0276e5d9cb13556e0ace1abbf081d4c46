public int[] evaluate(float fraction, int[] startValue, int[] endValue){
    int[] array = mArray;
    if (array == null) {
        array = new int[startValue.length];
    }
    for (int i = 0; i < array.length; i++) {
        int start = startValue[i];
        int end = endValue[i];
        array[i] = (int) (start + (fraction * (end - start)));
    }
    return array;
}