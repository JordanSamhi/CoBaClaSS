public SoftApConfiguration restoreSoftApBackupData(byte[] data){
    enforceNetworkSettingsPermission();
    mLog.info("restoreSoftApBackupData uid=%").c(Binder.getCallingUid()).flush();
    SoftApConfiguration softApConfig = mSoftApBackupRestore.retrieveSoftApConfigurationFromBackupData(data);
    if (softApConfig != null) {
        mWifiThreadRunner.post(() -> mWifiApConfigStore.setApConfiguration(mWifiApConfigStore.resetToDefaultForUnsupportedConfig(softApConfig)));
        Log.d(TAG, "Restored soft ap backup data");
    }
    return softApConfig;
}