public StringBuffer format(double number, StringBuffer toAppendTo, FieldPosition status){
    int i;
    for (i = 0; i < choiceLimits.length; ++i) {
        if (!(number >= choiceLimits[i])) {
            break;
        }
    }
    --i;
    if (i < 0)
        i = 0;
    return toAppendTo.append(choiceFormats[i]);
}