public boolean regionMatches(int toffset, String other, int ooffset, int len){
    int to = toffset;
    int po = ooffset;
    if ((ooffset < 0) || (toffset < 0) || (toffset > (long) length() - len) || (ooffset > (long) other.length() - len)) {
        return false;
    }
    while (len-- > 0) {
        if (charAt(to++) != other.charAt(po++)) {
            return false;
        }
    }
    return true;
}