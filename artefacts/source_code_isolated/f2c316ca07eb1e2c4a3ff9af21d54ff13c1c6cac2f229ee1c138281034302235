public static ViewNode sanitizeUrlBar(@NonNull AssistStructure structure, @NonNull String[] urlBarIds){
    final ViewNode urlBarNode = findViewNode(structure, (node) -> {
        return ArrayUtils.contains(urlBarIds, node.getIdEntry());
    });
    if (urlBarNode != null) {
        final String domain = urlBarNode.getText().toString();
        if (domain.isEmpty()) {
            if (sDebug)
                Slog.d(TAG, "sanitizeUrlBar(): empty on " + urlBarNode.getIdEntry());
            return null;
        }
        urlBarNode.setWebDomain(domain);
        if (sDebug) {
            Slog.d(TAG, "sanitizeUrlBar(): id=" + urlBarNode.getIdEntry() + ", domain=" + urlBarNode.getWebDomain());
        }
    }
    return urlBarNode;
}