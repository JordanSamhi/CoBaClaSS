public void addTile(ComponentName tile, boolean end){
    String spec = CustomTile.toSpec(tile);
    if (!mTileSpecs.contains(spec)) {
        List<String> newSpecs = new ArrayList<>(mTileSpecs);
        if (end) {
            newSpecs.add(spec);
        } else {
            newSpecs.add(0, spec);
        }
        changeTiles(mTileSpecs, newSpecs);
    }
}