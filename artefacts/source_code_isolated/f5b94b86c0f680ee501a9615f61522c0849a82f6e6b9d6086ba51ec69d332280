public static boolean bucketEquals(@Nullable WifiNative.BucketSettings bucket1, @Nullable WifiNative.BucketSettings bucket2){
    if (bucket1 == null || bucket2 == null)
        return false;
    if (bucket1 == bucket2)
        return true;
    if (bucket1.bucket != bucket2.bucket)
        return false;
    if (bucket1.band != bucket2.band)
        return false;
    if (bucket1.period_ms != bucket2.period_ms)
        return false;
    if (bucket1.report_events != bucket2.report_events)
        return false;
    if (bucket1.num_channels != bucket2.num_channels)
        return false;
    for (int c = 0; c < bucket1.num_channels; c++) {
        if (!channelEquals(bucket1.channels[c], bucket2.channels[c])) {
            return false;
        }
    }
    return true;
}