public boolean disableNetwork(int netId, String packageName){
    if (enforceChangePermission(packageName) != MODE_ALLOWED) {
        return false;
    }
    if (!isTargetSdkLessThanQOrPrivileged(packageName, Binder.getCallingPid(), Binder.getCallingUid())) {
        mLog.info("disableNetwork not allowed for uid=%").c(Binder.getCallingUid()).flush();
        return false;
    }
    int callingUid = Binder.getCallingUid();
    mLog.info("disableNetwork uid=%").c(callingUid).flush();
    return mWifiThreadRunner.call(() -> mWifiConfigManager.disableNetwork(netId, callingUid, packageName), false);
}