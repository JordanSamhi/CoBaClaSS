public final List<T> readParcelableList(@NonNull List<T> list, @Nullable ClassLoader cl){
    final int N = readInt();
    if (N == -1) {
        list.clear();
        return list;
    }
    final int M = list.size();
    int i = 0;
    for (; i < M && i < N; i++) {
        list.set(i, (T) readParcelable(cl));
    }
    for (; i < N; i++) {
        list.add((T) readParcelable(cl));
    }
    for (; i < M; i++) {
        list.remove(N);
    }
    return list;
}