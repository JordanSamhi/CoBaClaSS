public boolean equals(@Nullable Object obj){
    if (this == obj) {
        return true;
    }
    if (obj == null || getClass() != obj.getClass()) {
        return false;
    }
    UiccSlotInfo that = (UiccSlotInfo) obj;
    return (mIsActive == that.mIsActive) && (mIsEuicc == that.mIsEuicc) && (Objects.equals(mCardId, that.mCardId)) && (mCardStateInfo == that.mCardStateInfo) && (mLogicalSlotIdx == that.mLogicalSlotIdx) && (mIsExtendedApduSupported == that.mIsExtendedApduSupported) && (mIsRemovable == that.mIsRemovable);
}