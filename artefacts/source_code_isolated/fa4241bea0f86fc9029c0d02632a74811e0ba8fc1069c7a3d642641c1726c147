public long getAllowedNetworkTypes(){
    try {
        ITelephony telephony = getITelephony();
        if (telephony != null) {
            return telephony.getAllowedNetworkTypes(getSubId());
        }
    } catch (RemoteException ex) {
        Rlog.e(TAG, "getAllowedNetworkTypes RemoteException", ex);
    }
    return -1;
}