public boolean equals(Object obj){
    if (this == obj) {
        return true;
    }
    if (!(obj instanceof WifiAwareNetworkInfo)) {
        return false;
    }
    WifiAwareNetworkInfo lhs = (WifiAwareNetworkInfo) obj;
    return Objects.equals(mIpv6Addr, lhs.mIpv6Addr) && mPort == lhs.mPort && mTransportProtocol == lhs.mTransportProtocol;
}