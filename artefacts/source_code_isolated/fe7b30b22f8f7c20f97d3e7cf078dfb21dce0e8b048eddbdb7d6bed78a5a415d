public void add(@NonNull E entry){
    if (entry == null) {
        return;
    }
    int index = mLinkedList.indexOf(entry);
    if (index >= 0) {
        mLinkedList.remove(index);
    }
    mLinkedList.addFirst(entry);
    while (mLinkedList.size() > mSize) {
        mLinkedList.removeLast();
    }
}