public boolean removeNetwork(int netId, String packageName){
    if (enforceChangePermission(packageName) != MODE_ALLOWED) {
        return false;
    }
    if (!isTargetSdkLessThanQOrPrivileged(packageName, Binder.getCallingPid(), Binder.getCallingUid())) {
        mLog.info("removeNetwork not allowed for uid=%").c(Binder.getCallingUid()).flush();
        return false;
    }
    int callingUid = Binder.getCallingUid();
    mLog.info("removeNetwork uid=%").c(callingUid).flush();
    return mWifiThreadRunner.call(() -> mWifiConfigManager.removeNetwork(netId, callingUid, packageName), false);
}