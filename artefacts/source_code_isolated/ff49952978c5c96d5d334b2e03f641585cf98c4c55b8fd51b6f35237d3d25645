public boolean equals(Object o){
    if (this == o)
        return true;
    if (o == null || getClass() != o.getClass())
        return false;
    final AudioMix that = (AudioMix) o;
    return (this.mRouteFlags == that.mRouteFlags) && (this.mRule == that.mRule) && (this.mMixType == that.mMixType) && (this.mFormat == that.mFormat);
}